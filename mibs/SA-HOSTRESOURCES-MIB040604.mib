		-- 1.3.6.1.4.1.1429.9.1.12.2.3.8
		saHrProcessor OBJECT-IDENTITY
			STATUS current
			DESCRIPTION 
				"Processor Device Information"
			::= { saHrDevice 8 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.8.1
		saHrProcessorThreadReadyTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SaHrProcessorThreadReadyEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Information regarding the number of threads ready to run.
				
				
				If this entry is associated with a specific hrDeviceEntry and/or
				hrProcessorEntry from the HOST-RESOURCES-MIB, then saHrProcessorThreadReadyIndex
				must match the appropriate hrDeviceIndex."
			::= { saHrProcessor 1 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.8.1.1
		saHrProcessorThreadReadyEntry OBJECT-TYPE
			SYNTAX SaHrProcessorThreadReadyEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A row in the table.
				
				If this entry is associated with a specific hrDeviceEntry and/or
				hrProcessorEntry from the HOST-RESOURCES-MIB, then saHrProcessorThreadReadyIndex
				must match the appropriate hrDeviceIndex."
			INDEX { saHrProcessorThreadReadyIndex }
			::= { saHrProcessorThreadReadyTable 1 }

		
		SaHrProcessorThreadReadyEntry ::=
			SEQUENCE { 
				saHrProcessorThreadReadyIndex
					Integer32,
				saHrProcessorThreadReadyAvgOneMin
					Unsigned32,
				saHrProcessorThreadReadyAvgFiveMin
					Unsigned32,
				saHrProcessorThreadReadyAvgFifteenMin
					Unsigned32
			 }

		-- 1.3.6.1.4.1.1429.9.1.12.2.3.8.1.1.1
		saHrProcessorThreadReadyIndex OBJECT-TYPE
			SYNTAX Integer32 (1..2147483647)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The index of the processor for which this entry applies.
				
				If this entry is associated with a specific hrDeviceEntry and/or
				hrProcessorEntry from the HOST-RESOURCES-MIB, then saHrProcessorThreadReadyIndex
				must match the appropriate hrDeviceIndex."
			::= { saHrProcessorThreadReadyEntry 1 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.8.1.1.2
		saHrProcessorThreadReadyAvgOneMin OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "0.001"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The average number of threads ready to run over the past minute.
				The LS Bit is 0.001."
			::= { saHrProcessorThreadReadyEntry 2 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.8.1.1.3
		saHrProcessorThreadReadyAvgFiveMin OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "0.001"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The average number of threads ready to run over the past five minutes.
				The LS Bit is 0.001."
			::= { saHrProcessorThreadReadyEntry 3 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.8.1.1.4
		saHrProcessorThreadReadyAvgFifteenMin OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "0.001"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The average number of threads ready to run over the past fifteen minutes.
				The LS Bit is 0.001."
			::= { saHrProcessorThreadReadyEntry 4 }

		
