		saHrFsHealthTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SaHrFsHealthEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"File system health details.
				
				Coresponds to an entry in hrFSEntry in the 
				HOST-RESOURCES-MIB."
			::= { saHrDevice 7 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.7.1
		saHrFsHealthEntry OBJECT-TYPE
			SYNTAX SaHrFsHealthEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the table.
				
				Coresponds to an entry in hrFSEntry in the 
				HOST-RESOURCES-MIB."
			AUGMENTS { hrFSEntry }
			::= { saHrFsHealthTable 1 }

		
		SaHrFsHealthEntry ::=
			SEQUENCE { 
				saHrFsHealthDeletedFiles
					Unsigned32,
				saHrFsHealthCrossLinkedFiles
					Unsigned32,
				saHrFsHealthLostClusters
					Unsigned32,
				saHrFsHealthBadClusters
					Unsigned32
			 }

