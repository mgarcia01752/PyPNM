			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The value of sysUpTime when the last write failed for this location."
			::= { saHrNvmWriteStatEntry 4 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.4.3
		saHrNvmWriteTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SaHrNvmWriteEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"NVM Write Statistics Summary. This table summarizes the
				NVM writes recorded by the agent. It provides the maximum
				number of possible entries in the saHrNvmWriteStatTable and
				also the summary of successful and unsuccessful writes to 
				an NVM device."
			::= { saHrNvm 3 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.4.3.1
		saHrNvmWriteEntry OBJECT-TYPE
			SYNTAX SaHrNvmWriteEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the table."
			INDEX { hrDeviceIndex }
			::= { saHrNvmWriteTable 1 }

		
		SaHrNvmWriteEntry ::=
			SEQUENCE { 
				saHrNvmWriteCumulativeSuccesses
					Gauge32,
				saHrNvmWriteStatMaxEntries
					Gauge32,
				saHrNvmWriteStatCumulativeExcessFailures
					Gauge32
			 }

		-- 1.3.6.1.4.1.1429.9.1.12.2.3.4.3.1.1
		saHrNvmWriteCumulativeSuccesses OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The total number of NVM writes that have succeeded for this device."
			::= { saHrNvmWriteEntry 1 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.4.3.1.2
		saHrNvmWriteStatMaxEntries OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The maximum number of entries that this device will provide 
				in saHrNvmWriteStatTable"
			::= { saHrNvmWriteEntry 2 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.4.3.1.3
		saHrNvmWriteStatCumulativeExcessFailures OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The total number of NVM writes that have failed in excess 
				of what is recorded in the saHrNvmWriteStatTable for this 
				device. If this is non-zero, then more locations have failed 
				than what is reported in the table.
				"
			::= { saHrNvmWriteEntry 3 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.5
		saHrDeviceIdentityTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SaHrDeviceIdentityEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table provides identification and version information for
				coresponding devices listed in the hrDeviceTable."
			::= { saHrDevice 5 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.5.1
		saHrDeviceIdentityEntry OBJECT-TYPE
			SYNTAX SaHrDeviceIdentityEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the table."
			INDEX { hrDeviceIndex }
			::= { saHrDeviceIdentityTable 1 }

		
		SaHrDeviceIdentityEntry ::=
			SEQUENCE { 
				saHrDeviceIdentityModelNumber
					DisplayString,
				saHrDeviceIdentitySerialNumber
					DisplayString,
				saHrDeviceIdentityFirmwareVersion
					DisplayString
			 }

		-- 1.3.6.1.4.1.1429.9.1.12.2.3.5.1.1
		saHrDeviceIdentityModelNumber OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The model number of the device."
			::= { saHrDeviceIdentityEntry 1 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.5.1.2
		saHrDeviceIdentitySerialNumber OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The serial number of the device."
			::= { saHrDeviceIdentityEntry 2 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.5.1.3
		saHrDeviceIdentityFirmwareVersion OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The Firmware Version Number of the device."
			::= { saHrDeviceIdentityEntry 3 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.6
		saHrFsTypes OBJECT-IDENTITY
			STATUS current
			DESCRIPTION 
				"This subtree provides the identity of Scientific-Atlanta
				proprietary file systems.
				
				Values in this subtree are used to identify a Scientifi-Atlanta
				file system in the hrFSType column of the hrFSTable of the HOST-RESOURCES-MIB."
			::= { saHrDevice 6 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.6.1
		saHrFsItfs OBJECT-IDENTITY
			STATUS current
			DESCRIPTION 
				"The Scientific-Atlanta ITFS.
				
				This value is used to identify the ITFS as the hrFSType
				in the hrFSTable of the HOST-RESOURCES-MIB."
			::= { saHrFsTypes 1 }

		
		-- 1.3.6.1.4.1.1429.9.1.12.2.3.6.2
		saHrFsAvfs OBJECT-IDENTITY
			STATUS current
			DESCRIPTION 
				"The Scientific-Atlanta AVFS
				
				This value is used to identify the AVFS as the hrFSType
				in the hrFSTable of the HOST-RESOURCES-MIB.."
			::= { saHrFsTypes 2 }

		
