    fwRemoteManagement  OBJECT IDENTIFIER ::= { hitronRgFw 3 }

        -- external->internal Remote Management
        fwRemoteManagementExt OBJECT IDENTIFIER ::= { fwRemoteManagement 1 }

            fwRemoteManagementExtEnable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/disable external to internal remote management."
                DEFVAL      { true }
                ::= { fwRemoteManagementExt 1 }

            -- Clear all external hosts in current Table.
            fwRemoteManagementExtHostsClear  OBJECT-TYPE
                SYNTAX      FirewallControlTetrad
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "disable(1)     Do nothing.
                     ipv4-only(2)   Clear all the IPv4 rules.
                     ipv6-only(3)   Clear all the IPv6 rules.
                     both(4)        Clear all the rules in current table."
                DEFVAL      { disable }
                ::= { fwRemoteManagementExt 2 }

            fwRemoteManagementExtAllowAll OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Set true to allow all remote hosts,
                     set false to block all remote hosts
                     except which in allow list."
                DEFVAL      { false }
                ::= { fwRemoteManagementExt 3 }

            -- Rules Table
            fwRemoteManagementExtRulesTable OBJECT-TYPE
                SYNTAX      SEQUENCE OF FwRemoteManagementExtRulesEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
