                    INDEX       { fwFilterPubIPRulesIndex }
                    ::= { fwFilterPubIPRulesTable 1 }

                FwFilterPubIPRulesEntry ::=
                    SEQUENCE {
                        fwFilterPubIPRulesIndex       Integer32,
