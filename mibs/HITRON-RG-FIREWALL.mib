                ::= { fwFilterPubIP 1 }

            -- Clear all the rules in current Table.
            fwFilterPubIPRulesClear  OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Set true to clear all the rules in current table.
                     Set false will do nothing."
                DEFVAL      { false }
                ::= { fwFilterPubIP 2 }

            -- Rules Table
            fwFilterPubIPRulesTable OBJECT-TYPE
                SYNTAX      SEQUENCE OF FwFilterPubIPRulesEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Public LAN access control rules table."
