HITRON-RG-FW-MIB  DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY
    , OBJECT-TYPE
    , NOTIFICATION-TYPE
    , enterprises
    , Integer32
    , IpAddress
    , Unsigned32
    , Counter32
        FROM SNMPv2-SMI -- RFC2578

    DisplayString
    , MacAddress
    , TruthValue
    , RowStatus
    , TimeStamp
    , DateAndTime
        FROM SNMPv2-TC  -- RFC2579

    InetAddressType
    , InetAddress
    , InetAddressPrefixLength
    , InetPortNumber
    , InetVersion
        FROM INET-ADDRESS-MIB -- RFC4001

    hitronRg
    , LanIfType
    , LanAddrTitle
    , WanAddrTitle
    , TransportProtocolType
    , FirewallControlTetrad
        FROM HITRON-RG-MIB

    ; -- End of IMPORTS

hitronRgFw  MODULE-IDENTITY
    LAST-UPDATED    "201301091639Z" -- Jan. 09, 2013
    ORGANIZATION    "Hitron Technologies, Inc."
    CONTACT-INFO
        "Liangfeng Wu
        Hitron Technologies, Inc.
        B1,11/F International Science Park,
        No.1355, JinJi Lake Avenue, Suzhou Industrial Park, 215123
        P.R.C.
        Phone:  +86-512-62653066 Ext.8120
        E-mail: wuliangfeng@sz.hitrontech.com

        Haibo Xi
        Hitron Technologies, Inc.
        B1,11/F International Science Park,
        No.1355, JinJi Lake Avenue, Suzhou Industrial Park, 215123
        P.R.C.
        Phone:  +86-512-62653066 Ext.8148
        E-mail: xihaibo@sz.hitrontech.com"

    DESCRIPTION
        "MIB module for Firewall configuration, the 'Firewall' concept here is
        very general, involves most of the functions that will inspect or
        modify the network packets. And they are divided into three categories:
            NAT, Filter, and RemoteManagement. "
    
    REVISION    "201309262019Z" -- Sep. 26, 2013
    DESCRIPTION
        "Add fwNatSource."

    REVISION    "201301091639Z" -- Jan. 09, 2013
    DESCRIPTION
        "Change fwFilterLevelCusRulesType's MAX-ACCESS to read-only."

    REVISION    "201209051532Z" -- Sep. 05, 2012
    DESCRIPTION
        "Initial Version"

    ::= { hitronRg 19 }

    -- NAT
    fwNat   OBJECT IDENTIFIER ::= { hitronRgFw 1 }
        
        fwNatEnable OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Enable/disbale all the NAT function."
            ::= { fwNat 1 }

        -- DMZ
        fwNatDmz  OBJECT IDENTIFIER ::= { fwNat 2 }

            fwNatDmzEnable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/disable DMZ."
                ::= { fwNatDmz 1 }

            fwNatDmzHost    OBJECT-TYPE
                SYNTAX      IpAddress
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Configure the DMZ host."
                ::= { fwNatDmz 2 }

        -- Port Forwarding
        fwNatPortForwarding OBJECT IDENTIFIER ::= { fwNat 3 }

            fwNatPFEnable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/Disable Port Forwarding."
                ::= { fwNatPortForwarding 1 }

            -- Clear all the port forwarding rules in current Table.
            fwNatPFRulesClear  OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Set true to clear all the rules in current table.
                     Set false will do nothing."
                DEFVAL      { false }
                ::= { fwNatPortForwarding 2 }

            -- Port forwarding rules table
            fwNatPFRulesTable   OBJECT-TYPE
                SYNTAX      SEQUENCE OF FwNatPFRulesEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Port forwarding rules table."
                ::= { fwNatPortForwarding 3 }

                fwNatPFRulesEntry OBJECT-TYPE
                    SYNTAX      FwNatPFRulesEntry
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "Entry of the fwNatPFRulesTable."
                    INDEX       { fwNatPFRulesIndex }
                    ::= { fwNatPFRulesTable 1 }

                FwNatPFRulesEntry ::=
                    SEQUENCE {
                        fwNatPFRulesIndex             Integer32,
                        fwNatPFRulesRowStatus         RowStatus,
                        fwNatPFRulesEnable            TruthValue,
                        fwNatPFRulesDescription       DisplayString,
                        fwNatPFRulesProtocol          TransportProtocolType,
                        fwNatPFRulesRemoteHostStart   IpAddress,
                        fwNatPFRulesRemoteHostEnd     IpAddress,
                        fwNatPFRulesPubPortStart      InetPortNumber,
                        fwNatPFRulesPubPortEnd        InetPortNumber,
                        fwNatPFRulesLocalHost         IpAddress,
                        fwNatPFRulesPrivPortStart     InetPortNumber,
                        fwNatPFRulesPrivPortEnd       InetPortNumber,
                        fwNatPFRulesMappingType       INTEGER
                    }

                fwNatPFRulesIndex OBJECT-TYPE
                    SYNTAX      Integer32
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "Index of the rules"
                    ::= { fwNatPFRulesEntry 1 }

                fwNatPFRulesRowStatus OBJECT-TYPE
                    SYNTAX      RowStatus
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "RowStatus used to manage the creation and deletion
                         of conceptual row. "
                    ::= { fwNatPFRulesEntry 2 }

                fwNatPFRulesEnable OBJECT-TYPE
                    SYNTAX      TruthValue
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "This value indicates whether this rule is active."
                    DEFVAL      { true }
                    ::= { fwNatPFRulesEntry 3 }

                fwNatPFRulesDescription OBJECT-TYPE
                    SYNTAX      DisplayString
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "A simple description to this rule."
                    ::= { fwNatPFRulesEntry 4 }

                fwNatPFRulesProtocol OBJECT-TYPE
                    SYNTAX      TransportProtocolType
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Protocol type, could be tcp(1), udp(2) and
                         tcpudp(3)."
                    DEFVAL      { tcp }
                    ::= { fwNatPFRulesEntry 5 }

                fwNatPFRulesRemoteHostStart OBJECT-TYPE
                    SYNTAX      IpAddress
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Combined with fwNatPFRulesRemoteHostEnd to
                         represent the host range that allowed to this
                         port forwarding rule, value '0.0.0.0' indicates
                         any host."
                    ::= { fwNatPFRulesEntry 6 }

                fwNatPFRulesRemoteHostEnd OBJECT-TYPE
                    SYNTAX      IpAddress
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Combined with fwNatPFRulesRemoteHostStart to
                         represent the host range that allowed to this
                         port forwarding rule, value '0.0.0.0' indicates
                         single host that set in fwNatPFRulesRemoteHostStart."
                    ::= { fwNatPFRulesEntry 7 }

                fwNatPFRulesPubPortStart OBJECT-TYPE
                    SYNTAX      InetPortNumber
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Start number of the public port."
                    ::= { fwNatPFRulesEntry 8 }

                fwNatPFRulesPubPortEnd OBJECT-TYPE
                    SYNTAX      InetPortNumber
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "End number of the public port."
                    ::= { fwNatPFRulesEntry 9 }

                fwNatPFRulesLocalHost OBJECT-TYPE
                    SYNTAX      IpAddress
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "The local destination host."
                    ::= { fwNatPFRulesEntry 10 }

                fwNatPFRulesPrivPortStart OBJECT-TYPE
                    SYNTAX      InetPortNumber
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Start number of the private port."
                    ::= { fwNatPFRulesEntry 11 }

                fwNatPFRulesPrivPortEnd OBJECT-TYPE
                    SYNTAX      InetPortNumber
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "End number of the private port. set 0 or same
                         value as start port to indicate single port."
                    ::= { fwNatPFRulesEntry 12 }

                fwNatPFRulesMappingType OBJECT-TYPE
                    SYNTAX      INTEGER {
                                    random(0),
                                    range(1),
                                    n-to-1(2)
                                }
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "The port mapping type."
                    DEFVAL      { range }
                    ::= { fwNatPFRulesEntry 13 }

        -- IP2IP Mapping
        fwNatIp2IpMapping OBJECT IDENTIFIER ::= { fwNat 4 }

            fwNatIp2IpEnable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/Disable IP2IP Mapping."
                ::= { fwNatIp2IpMapping 1 }

            -- Clear all the IP2IP mapping rules in current Table.
            fwNatIp2IpRulesClear  OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Set true to clear all the rules in current table.
                     Set false will do nothing."
                DEFVAL      { false }
                ::= { fwNatIp2IpMapping 2 }

            -- IP2IP mapping rules table
            fwNatIp2IpRulesTable   OBJECT-TYPE
                SYNTAX      SEQUENCE OF FwNatIp2IpRulesEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "IP2IP mapping rules table."
                ::= { fwNatIp2IpMapping 3 }

                fwNatIp2IpRulesEntry OBJECT-TYPE
                    SYNTAX      FwNatIp2IpRulesEntry
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "Entry of the fwNatIp2IpRulesTable."
                    INDEX       { fwNatIp2IpRulesIndex }
                    ::= { fwNatIp2IpRulesTable 1 }

                FwNatIp2IpRulesEntry ::=
                    SEQUENCE {
                        fwNatIp2IpRulesIndex                Integer32,
                        fwNatIp2IpRulesRowStatus            RowStatus,
                        fwNatIp2IpRulesEnable               TruthValue,
                        fwNatIp2IpRulesPublicIp             IpAddress,
                        fwNatIp2IpRulesPrivateIp            IpAddress,
                        fwNatIp2IpRulesPrivateIpPrefixLen   Integer32
                    }

                fwNatIp2IpRulesIndex OBJECT-TYPE
                    SYNTAX      Integer32
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "Index of the rules"
                    ::= { fwNatIp2IpRulesEntry 1 }

                fwNatIp2IpRulesRowStatus OBJECT-TYPE
                    SYNTAX      RowStatus
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "RowStatus used to manage the creation and deletion
                         of conceptual row. "
                    ::= { fwNatIp2IpRulesEntry 2 }

                fwNatIp2IpRulesEnable OBJECT-TYPE
                    SYNTAX      TruthValue
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "This value indicates whether this rule is active."
                    DEFVAL      { true }
                    ::= { fwNatIp2IpRulesEntry 3 }

                fwNatIp2IpRulesPublicIp OBJECT-TYPE
                    SYNTAX      IpAddress
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Public IP address."
                    ::= { fwNatIp2IpRulesEntry 4 }

                fwNatIp2IpRulesPrivateIp OBJECT-TYPE
                    SYNTAX      IpAddress
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Private IP address. Format is A.B.C.D[/prefix]."
                    ::= { fwNatIp2IpRulesEntry 5 }

                fwNatIp2IpRulesPrivateIpPrefixLen OBJECT-TYPE
                    SYNTAX      Integer32 (24..32)
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Prefix length of private IP address."
                    ::= { fwNatIp2IpRulesEntry 6 }

        -- Port Triggering
        fwNatPortTriggering  OBJECT IDENTIFIER ::= { fwNat 5 }

            fwNatPTGEnable  OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/Disable Port Triggering.

                     Port triggering is a configuration option on a
                     NAT-enabled router that allows a host machine to
                     dynamically and automatically forward a specific port
                     back to itself. Port triggering opens an incoming port
                     when your computer is using a specified outgoing port
                     for specific traffic."
                ::= { fwNatPortTriggering 1 }

            -- Clear all the port triggering rules in current Table.
            fwNatPTGRulesClear  OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Set true to clear all the rules in current table.
                     Set false will do nothing."
                DEFVAL      { false }
                ::= { fwNatPortTriggering 2 }

            -- Port Triggering Rules Table
            fwNatPTGRulesTable OBJECT-TYPE
                SYNTAX      SEQUENCE OF FwNatPTGRulesEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Port triggering rules table."
                ::= { fwNatPortTriggering 3 }

                fwNatPTGRulesEntry OBJECT-TYPE
                    SYNTAX      FwNatPTGRulesEntry
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "Entry of the fwNatPTGRulesTable."
                    INDEX       { fwNatPTGRulesIndex }
                    ::= { fwNatPTGRulesTable 1 }

                FwNatPTGRulesEntry ::=
                    SEQUENCE {
                        fwNatPTGRulesIndex           Integer32,
                        fwNatPTGRulesRowStatus       RowStatus,
                        fwNatPTGRulesEnable          TruthValue,
                        fwNatPTGRulesDescription     DisplayString,
                        fwNatPTGRulesProtocol        TransportProtocolType,
                        fwNatPTGRulesTrigPortStart   InetPortNumber,
                        fwNatPTGRulesTrigPortEnd     InetPortNumber,
                        fwNatPTGRulesTargPortStart   InetPortNumber,
                        fwNatPTGRulesTargPortEnd     InetPortNumber,
                        fwNatPTGRulesTimeout         Unsigned32,
                        fwNatPTGRulesAllowThird      TruthValue
                    }

               fwNatPTGRulesIndex OBJECT-TYPE
                    SYNTAX      Integer32
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "Index of the rules."
                    ::= { fwNatPTGRulesEntry 1 }

                fwNatPTGRulesRowStatus OBJECT-TYPE
                    SYNTAX      RowStatus
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "RowStatus used to manage the creation and deletion
                         of conceptual row."
                    ::= { fwNatPTGRulesEntry 2 }

                fwNatPTGRulesEnable OBJECT-TYPE
                    SYNTAX      TruthValue
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Bool value indicate whether this rule is active."
                    DEFVAL      { true }
                    ::= { fwNatPTGRulesEntry 3 }

                fwNatPTGRulesDescription OBJECT-TYPE
                    SYNTAX      DisplayString
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Description for this rule."
                    ::= { fwNatPTGRulesEntry 4 }

                fwNatPTGRulesProtocol OBJECT-TYPE
                    SYNTAX      TransportProtocolType
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Protocol type, could be tcp(1), udp(2) and
                         tcpudp(3)."
                    ::= { fwNatPTGRulesEntry 5 }

                fwNatPTGRulesTrigPortStart OBJECT-TYPE
                    SYNTAX      InetPortNumber
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Start number of the triggering ports."
                    ::= { fwNatPTGRulesEntry 6 }

                fwNatPTGRulesTrigPortEnd OBJECT-TYPE
                    SYNTAX      InetPortNumber
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "End number of the triggering ports."
                    ::= { fwNatPTGRulesEntry 7 }

                fwNatPTGRulesTargPortStart OBJECT-TYPE
                    SYNTAX      InetPortNumber
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Start number of the target inbound ports."
                    ::= { fwNatPTGRulesEntry 8 }

                fwNatPTGRulesTargPortEnd OBJECT-TYPE
                    SYNTAX      InetPortNumber
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "End number of the target inbound ports."
                    ::= { fwNatPTGRulesEntry 9 }

                fwNatPTGRulesTimeout OBJECT-TYPE
                    SYNTAX      Unsigned32 (50..30000)
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "The Timeout of this rule."
                    ::= { fwNatPTGRulesEntry 10 }

                fwNatPTGRulesAllowThird OBJECT-TYPE
                    SYNTAX      TruthValue
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Bool value indicates whether this rule is allowed
                         a third host to join in."
                    ::= { fwNatPTGRulesEntry 11 }
        
        -- NAT Source
        fwNatSource OBJECT-TYPE
            SYNTAX      INTEGER {
                    natOnDhcp(0),
                    natOnStatic(1)
            }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION 
                "Select the IP address used to perform NAT, two options:  
                 - 'natOnDhcp(0)': Perform NAT on the IP address obtained
                        via DHCP;
                 - 'natOnStatic(1)': Perform NAT on the Static IP address
                        assigned manually."
            ::= { fwNat 6 }

    -- Filter
    fwFilter OBJECT IDENTIFIER ::= { hitronRgFw 2 }

        fwFilterEnable OBJECT-TYPE
            SYNTAX      FirewallControlTetrad
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "disable(1)     Disable the whole filter module.
                 ipv4-only(2)   Enable the IPv4 filter module.
                 ipv6-only(3)   Enable the IPv6 filter module.
                 both(4)        Enable the whole filter module."
            DEFVAL      { both }
            ::= { fwFilter 1 }

        -- Public DMZ, IP Hole..
        fwFilterDmz OBJECT IDENTIFIER ::= { fwFilter 2 }

            fwFilterDmzEnable OBJECT-TYPE
                SYNTAX      FirewallControlTetrad
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "disable(1)     Disable the whole Public DMZ(IP Hole) filter.
                     ipv4-only(2)   Enable the IPv4 Public DMZ filter.
                     ipv6-only(3)   Enable the IPv6 Public DMZ filter.
                     both(4)        Enable the whole Public DMZ filter."
                DEFVAL      { both }
                ::= { fwFilterDmz 1 }

            -- Clear all the rules in current Table.
            fwFilterDmzRulesClear  OBJECT-TYPE
                SYNTAX      FirewallControlTetrad
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "disable(1)     Do nothing.
                     ipv4-only(2)   Clear all the IPv4 rules.
                     ipv6-only(3)   Clear all the IPv6 rules.
                     both(4)        Clear all the rules in current table."
                DEFVAL      { disable }
                ::= { fwFilterDmz 2 }

            -- Rules Table
            fwFilterDmzRulesTable OBJECT-TYPE
                SYNTAX      SEQUENCE OF FwFilterDmzRulesEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Public DMZ rules table."
                ::= { fwFilterDmz 3 }

                fwFilterDmzRulesEntry OBJECT-TYPE
                    SYNTAX      FwFilterDmzRulesEntry
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "Entry of the fwFilterDmzRulesTable."
                    INDEX       { fwFilterDmzRulesIndex }
                    ::= { fwFilterDmzRulesTable 1 }

                FwFilterDmzRulesEntry ::=
                    SEQUENCE {
                        fwFilterDmzRulesIndex       Integer32,
                        fwFilterDmzRulesRowStatus   RowStatus,
                        fwFilterDmzRulesEnable      TruthValue,
                        fwFilterDmzRulesAddrType    InetAddressType,
                        fwFilterDmzRulesAddr        InetAddress
                    }

                fwFilterDmzRulesIndex OBJECT-TYPE
                    SYNTAX      Integer32
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "Index of the rules."
                    ::= { fwFilterDmzRulesEntry 1 }

                fwFilterDmzRulesRowStatus OBJECT-TYPE
                    SYNTAX      RowStatus
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "RowStatus used to manage the creation and deletion
                         of conceptual row."
                    ::= { fwFilterDmzRulesEntry 2 }

                fwFilterDmzRulesEnable OBJECT-TYPE
                    SYNTAX      TruthValue
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Bool value indicates whether this rule is active."
                    ::= { fwFilterDmzRulesEntry 3 }

                fwFilterDmzRulesAddrType OBJECT-TYPE
                    SYNTAX      InetAddressType
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "Public IPv4 address or IPv6 address."
                    ::= { fwFilterDmzRulesEntry 4 }

                fwFilterDmzRulesAddr OBJECT-TYPE
                    SYNTAX      InetAddress
                    MAX-ACCESS  read-create
                    STATUS      current
                    DESCRIPTION
                        "The Internet address value of the DMZ host."
                    ::= { fwFilterDmzRulesEntry 5 }

        -- IDS
        fwFilterIds OBJECT IDENTIFIER ::= { fwFilter 3 }

            fwFilterIdsEnable OBJECT-TYPE
                SYNTAX      FirewallControlTetrad
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "disable(1)     Disable the whole IDS filter.
                     ipv4-only(2)   Enable the IPv4 IDS filter.
                     ipv6-only(3)   Enable the IPv6 IDS filter.
                     both(4)        Enable the whole IDS filter."
                DEFVAL      { both }
                ::= { fwFilterIds 1 }

        -- Port filter
        fwFilterPort OBJECT IDENTIFIER ::= { fwFilter 4 }

            -- External Port Allow
            fwFilterPortExt OBJECT IDENTIFIER ::= { fwFilterPort 1 }

                fwFilterPortExtEnable OBJECT-TYPE
                    SYNTAX      FirewallControlTetrad
                    MAX-ACCESS  read-only
                    STATUS      current
                    DESCRIPTION
                        "Get WAN-side Port filter status. Can't be set,
                         otherwise the SNMP will block itself."
                    DEFVAL      { both }
                    ::= { fwFilterPortExt 1 }

                -- Rules Table
                fwFilterPortExtRulesTable OBJECT-TYPE
                    SYNTAX      SEQUENCE OF FwFilterPortExtRulesEntry
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "External port allow rules table."
                    ::= { fwFilterPortExt 2 }

                    fwFilterPortExtRulesEntry OBJECT-TYPE
                        SYNTAX      FwFilterPortExtRulesEntry
                        MAX-ACCESS  not-accessible
                        STATUS      current
                        DESCRIPTION
                            "Entry of the fwFilterPortExtRulesTable."
                        INDEX       { fwFilterPortExtRulesIndex }
                        ::= { fwFilterPortExtRulesTable 1 }

                    FwFilterPortExtRulesEntry ::=
                        SEQUENCE {
                            fwFilterPortExtRulesIndex      Integer32,
                            fwFilterPortExtRulesEnable     TruthValue,
                            fwFilterPortExtRulesVersion    INTEGER,
                            fwFilterPortExtRulesProtocol   TransportProtocolType,
                            fwFilterPortExtRulesPort       InetPortNumber
                        }

                    fwFilterPortExtRulesIndex OBJECT-TYPE
                        SYNTAX      Integer32
                        MAX-ACCESS  not-accessible
                        STATUS      current
                        DESCRIPTION
                            "Index of the rules."
                        ::= { fwFilterPortExtRulesEntry 1 }

                    fwFilterPortExtRulesEnable OBJECT-TYPE
                        SYNTAX      TruthValue
                        MAX-ACCESS  read-only
                        STATUS      current
                        DESCRIPTION
                            "Bool value indicates whether this rule is active."
                        ::= { fwFilterPortExtRulesEntry 2 }

                    fwFilterPortExtRulesVersion OBJECT-TYPE
                        SYNTAX      INTEGER {
                                        unknown(0),
                                        ipv4(1),
                                        ipv6(2),
                                        both(3)
                                    }
                        MAX-ACCESS  read-only
                        STATUS      current
                        DESCRIPTION
                            "A value representing a version of the IP protocol.
                             unknown(0)  An unknown or unspecified version
                                         of the IP protocol.
                             ipv4(1)     The IPv4 protocol as defined in
                                         RFC 791 (STD 5).
                             ipv6(2)     The IPv6 protocol as defined in
                                         RFC 2460.
                             both(3)     Both IPv4 and IPv6 protocol."
                        ::= { fwFilterPortExtRulesEntry 3 }

                    fwFilterPortExtRulesProtocol OBJECT-TYPE
                        SYNTAX      TransportProtocolType
                        MAX-ACCESS  read-only
                        STATUS      current
                        DESCRIPTION
                            "Protocol type, could be tcp(1), udp(2) and
                             tcpudp(3)."
                        ::= { fwFilterPortExtRulesEntry 4 }

                    fwFilterPortExtRulesPort OBJECT-TYPE
                        SYNTAX      InetPortNumber
                        MAX-ACCESS  read-only
                        STATUS      current
                        DESCRIPTION
                            "Filter port number."
                        ::= { fwFilterPortExtRulesEntry 5 }

            -- Internal Port Block
            fwFilterPortInt OBJECT IDENTIFIER ::= { fwFilterPort 2 }

                fwFilterPortIntEnable OBJECT-TYPE
                    SYNTAX      FirewallControlTetrad
                    MAX-ACCESS  read-only
                    STATUS      current
                    DESCRIPTION
                        "Get LAN-side Port filter status."
                    DEFVAL      { disable }
                    ::= { fwFilterPortInt 1 }

                -- Rules Table
                fwFilterPortIntRulesTable OBJECT-TYPE
                    SYNTAX      SEQUENCE OF FwFilterPortIntRulesEntry
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "Internal Port Block rules table."
                    ::= { fwFilterPortInt 2 }

                    fwFilterPortIntRulesEntry OBJECT-TYPE
                        SYNTAX      FwFilterPortIntRulesEntry
                        MAX-ACCESS  not-accessible
                        STATUS      current
                        DESCRIPTION
                            "Entry of the fwFilterPortIntRulesTable."
                        INDEX       { fwFilterPortIntRulesIndex }
                        ::= { fwFilterPortIntRulesTable 1 }

                    FwFilterPortIntRulesEntry ::=
                        SEQUENCE {
                            fwFilterPortIntRulesIndex      Integer32,
                            fwFilterPortIntRulesEnable     TruthValue,
                            fwFilterPortIntRulesVersion    INTEGER,
                            fwFilterPortIntRulesProtocol   TransportProtocolType,
                            fwFilterPortIntRulesPort       InetPortNumber
                        }

                    fwFilterPortIntRulesIndex OBJECT-TYPE
                        SYNTAX      Integer32
                        MAX-ACCESS  not-accessible
                        STATUS      current
                        DESCRIPTION
                            "Index of the rules."
                        ::= { fwFilterPortIntRulesEntry 1 }

                    fwFilterPortIntRulesEnable OBJECT-TYPE
                        SYNTAX      TruthValue
                        MAX-ACCESS  read-create
                        STATUS      current
                        DESCRIPTION
                            "Bool value indicates whether this rule is active."
                        ::= { fwFilterPortIntRulesEntry 2 }

