HITRON-RG-NI-MIB  DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY
    , OBJECT-TYPE
    , NOTIFICATION-TYPE
    , enterprises
    , Integer32
    , IpAddress
    , Unsigned32
    , Counter32
        FROM SNMPv2-SMI -- RFC2578

    DisplayString
    , MacAddress
    , TruthValue
    , RowStatus
    , TimeStamp
    , DateAndTime
        FROM SNMPv2-TC  -- RFC2579
    
    InetAddressType
    , InetAddress
    , InetAddressPrefixLength
    , InetPortNumber
    , InetVersion
        FROM INET-ADDRESS-MIB -- RFC4001

    hitronRg
    , LanIfType
    , LanAddrTitle
    , WanAddrTitle
        FROM HITRON-RG-MIB
    
    ;  --End of IMPORTS

hitronRgNi  MODULE-IDENTITY
    LAST-UPDATED    "201210291943Z" -- Oct. 29, 2012
    ORGANIZATION    "Hitron Technologies, Inc."
    CONTACT-INFO
        "Liangfeng Wu
        Hitron Technologies, Inc.
        B1,11/F International Science Park,
        No.1355, JinJi Lake Avenue, Suzhou Industrial Park, 215123
        P.R.C.
        Phone:  +86-512-62653066 Ext.8120
        E-mail: wuliangfeng@sz.hitrontech.com

        Haibo Xi
        Hitron Technologies, Inc.
        B1,11/F International Science Park,
        No.1355, JinJi Lake Avenue, Suzhou Industrial Park, 215123
        P.R.C.
        Phone:  +86-512-62653066 Ext.8148
        E-mail: xihaibo@sz.hitrontech.com"

    DESCRIPTION
        "MIB module for Network Interface(NI) configuration"

    REVISION    "201210291943Z" -- Oct. 29, 2012
    DESCRIPTION
        "Because SixRd's Border Relay Address is IPv4 address,change 
         type to IpAddress"

    REVISION    "201210220001Z" -- Oct. 22, 2012
    DESCRIPTION
        "Remove netifLanRipDefaultMetric from netifLanRipEntry.
        Only Wan interface's metric is settable"

    REVISION    "201209290001Z" -- Sep. 29, 2012
    DESCRIPTION
        "Add netifWan and netifLanRip netifLanRipng"

    REVISION    "201209051505Z" -- Sep. 05, 2012
    DESCRIPTION
        "Initial Version"

    ::= { hitronRg 18 }

    -- LAN Interfaces
    netifLan    OBJECT IDENTIFIER  ::= { hitronRgNi 1 }

        -- Basic LAN Interface table
        netifLanBaseTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF NetifLanBaseEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "LAN interface table including the basic information."
            ::= { netifLan 1 }

            netifLanBaseEntry OBJECT-TYPE
                SYNTAX      NetifLanBaseEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Entry of the basic LAN interface table."
                INDEX       { netifLanBaseType,
                              netifLanBaseIndex }
                ::= { netifLanBaseTable 1 }

            NetifLanBaseEntry ::=
                SEQUENCE {
                    netifLanBaseType            LanIfType,
                    netifLanBaseIndex           Integer32,
                    netifLanBaseName            DisplayString,
                    netifLanBaseIpv6Enable      TruthValue,
                    netifLanBaseIgmpProxyEnable TruthValue,
                    netifLanBaseMldProxyEnable  TruthValue,
                    netifLanBaseIpv6RaDnsEnable TruthValue
                }
            
            netifLanBaseType  OBJECT-TYPE
                SYNTAX      LanIfType
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "LAN interface type, this value combined with the index
                    of this types of interface, used to uniquely identify a
                    LAN interface."
                ::= { netifLanBaseEntry 1 }

            netifLanBaseIndex OBJECT-TYPE
                SYNTAX      Integer32
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Index value based on the RG model, the interface 
                    identified by a particular value of this index is not 
                    the same as identified by the same value of ifIndex."
                ::= { netifLanBaseEntry 2 }

            netifLanBaseName  OBJECT-TYPE
                SYNTAX      DisplayString
                MAX-ACCESS  read-only   -- change to read-write in future
                STATUS      current
                DESCRIPTION
                    "The name of a particular interface."
                ::= { netifLanBaseEntry 3 }
            
            netifLanBaseIpv6Enable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/Disable IPv6 on this LAN interface."
                ::= { netifLanBaseEntry 4 }
            
            netifLanBaseIgmpProxyEnable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/Disable the IGMP proxy on this LAN interface."
                ::= { netifLanBaseEntry 5 }
            
            netifLanBaseMldProxyEnable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/Disable the MLD proxy on this LAN interface."
                ::= { netifLanBaseEntry 6 }

            netifLanBaseIpv6RaDnsEnable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/Disable the manually configured DNS(static 
                     DNS) for Router Advertisement on this interface.
                     The static DNS has priority over the one obtained via
                     DHCP on WAN interface."
                ::= { netifLanBaseEntry 7 }

        -- LAN Interface Address Table(child table of LAN interface Table)
        netifLanAddrTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF NetifLanAddrEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "This table includes all the addresses that configured on 
                 this interface."
            ::= { netifLan 2 }
            
            netifLanAddrEntry OBJECT-TYPE
                SYNTAX      NetifLanAddrEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Entry of the netifLanAddrTable"
                INDEX       { netifLanBaseType,
                              netifLanBaseIndex, 
                              netifLanAddrTitle,
                              netifLanAddrIndex }
                ::= { netifLanAddrTable 1 }

            NetifLanAddrEntry ::=
                SEQUENCE {
                    netifLanAddrTitle           LanAddrTitle,
                    netifLanAddrIndex           Integer32,
                    netifLanAddrType            InetAddressType,
                    netifLanAddrValue           InetAddress,
                    netifLanAddrPrefixLength    InetAddressPrefixLength
                }
            
            netifLanAddrTitle OBJECT-TYPE
                SYNTAX      LanAddrTitle
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Titile of the address, check the TC of LanAddrTitle 
                    for details."
                ::= { netifLanAddrEntry 1 }

            netifLanAddrIndex OBJECT-TYPE
                SYNTAX      Integer32
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Index of addresses with the same title."
                ::= { netifLanAddrEntry 2 }

            netifLanAddrType OBJECT-TYPE
                SYNTAX      InetAddressType
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Internet address type of this entry."
                ::= { netifLanAddrEntry 3 }

            netifLanAddrValue OBJECT-TYPE
                SYNTAX      InetAddress
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "The value of Internet address."
                ::= { netifLanAddrEntry 4 }

            netifLanAddrPrefixLength OBJECT-TYPE
                SYNTAX      InetAddressPrefixLength
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "This value identifies the Internet network address 
                    prefix.  "
                ::= { netifLanAddrEntry 5 }

        -- LAN DHCPv4 Server Table(extension to the LAN interface Table)
        netifLanDhcpv4ServerTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF NetifLanDhcpv4ServerEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "LAN interface DHCPv4 Server table"
            ::= { netifLan 3 }

            netifLanDhcpv4ServerEntry OBJECT-TYPE
                SYNTAX      NetifLanDhcpv4ServerEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Entry of the DHCPv4 Server Table"
                AUGMENTS    { netifLanBaseEntry }
                ::= { netifLanDhcpv4ServerTable 1 }
    
            NetifLanDhcpv4ServerEntry ::= 
                SEQUENCE {
                    netifLanDhcpv4ServerEnable      TruthValue,
                    netifLanDhcpv4ServerPoolStart   IpAddress,
                    netifLanDhcpv4ServerPoolEnd     IpAddress,
                    netifLanDhcpv4ServerLeasetime   Unsigned32
                }
    
            netifLanDhcpv4ServerEnable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/disable the DHCPv4 Server on the particular 
                    LAN interface."
                ::= { netifLanDhcpv4ServerEntry 1 }
    
            netifLanDhcpv4ServerPoolStart OBJECT-TYPE
                SYNTAX      IpAddress
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Start IP address of the DHCPv4 pool"
                ::= { netifLanDhcpv4ServerEntry 2 }
    
            netifLanDhcpv4ServerPoolEnd OBJECT-TYPE
                SYNTAX      IpAddress
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "End IP address of the DHCPv4 Pool"
                ::= { netifLanDhcpv4ServerEntry 3 }
    
            netifLanDhcpv4ServerLeasetime OBJECT-TYPE
                SYNTAX      Unsigned32(180..4294967295)
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Lease time in seconds"
                ::= { netifLanDhcpv4ServerEntry 4 }

        -- IPv6 Router Advertisement Table(extension to the LAN Base Table)
        netifLanIpv6RaTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF NetifLanIpv6RaEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "LAN interface IPv6 RA Table"
            ::= { netifLan 4 }

            netifLanIpv6RaEntry OBJECT-TYPE
                SYNTAX      NetifLanIpv6RaEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Entry of the IPv6 RA Table"
                AUGMENTS    { netifLanBaseEntry }
                ::= { netifLanIpv6RaTable 1 }
    
            NetifLanIpv6RaEntry ::= 
                SEQUENCE {
                    netifLanIpv6RaEnable            TruthValue,
                    netifLanIpv6RaAdvIntvlMax       Unsigned32,
                    netifLanIpv6RaAdvIntvlMin       Unsigned32,
                    netifLanIpv6RaManaged           TruthValue,
                    netifLanIpv6RaOtherConfig       TruthValue,
                    netifLanIpv6RaLinkMTU           Unsigned32,
                    netifLanIpv6RaHopLimit          Unsigned32
                }

            netifLanIpv6RaEnable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/Disable Router Advertisement on this interface
                    ."
                ::= { netifLanIpv6RaEntry 1 }

            netifLanIpv6RaAdvIntvlMax OBJECT-TYPE
                SYNTAX      Unsigned32(4..1800)
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "The maximum time in seconds allowed between sending 
                     unsolicited multicast router advertisements from the 
                     interface, in seconds. "
                ::= { netifLanIpv6RaEntry 2 }

            netifLanIpv6RaAdvIntvlMin OBJECT-TYPE
                SYNTAX      Unsigned32(3..1350)
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "The minimum time in seconds allowed between sending 
                     unsolicited multicast router advertisements from the 
                     interface. The maximun value must be no greater than 
                     0.75 * netifLanIpv6RaAdvIntvlMax. "
                ::= { netifLanIpv6RaEntry 3 }

            netifLanIpv6RaManaged OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "If true, hosts use the administered (stateful) 
                    protocol for address autoconfiguration in addition to 
                    any addresses autoconfigured using stateless address 
                    autoconfiguration."
                ::= { netifLanIpv6RaEntry 4 }

            netifLanIpv6RaOtherConfig OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "If true, hosts use the administered (stateful) 
                    protocol for autoconfiguration of other (non-address) 
                    information. "
                ::= { netifLanIpv6RaEntry 5 }

            netifLanIpv6RaLinkMTU OBJECT-TYPE
                SYNTAX      Unsigned32(1280..1800)
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "The MTU option is used in router advertisement 
                    messages to insure that all nodes on a link use the 
                    same MTU value in those cases where the link MTU is 
                    not well known.  If specified, i.e. not 0, must not be 
                    smaller than 1280 and not greater than the maximum MTU 
                    allowed for this link (e.g. ethernet has a maximum MTU 
                    of 1500. See RFC 4864). "
                ::= { netifLanIpv6RaEntry 6 }

            netifLanIpv6RaHopLimit OBJECT-TYPE
                SYNTAX      Unsigned32(1..256)
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "The default value that should be placed in the Hop 
                    Count field of the IP header for outgoing (unicast) IP 
                    packets.  The value should be set to the current 
                    diameter of the Internet. The value zero means 
                    unspecified (by this router) ."
                ::= { netifLanIpv6RaEntry 7 }

        -- IPv6 Router Advertisement Manually Configured Prefix(extension)
        netifLanIpv6RaPrefixTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF NetifLanIpv6RaPrefixEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "LAN interface IPv6 RA Static Prefix Table"
            ::= { netifLan 5 }

            netifLanIpv6RaPrefixEntry OBJECT-TYPE
                SYNTAX      NetifLanIpv6RaPrefixEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Entry of the IPv6 RA Static Prefix Table"
                AUGMENTS    { netifLanBaseEntry }
                ::= { netifLanIpv6RaPrefixTable 1 }
    
            NetifLanIpv6RaPrefixEntry ::= 
                SEQUENCE {
                    netifLanIpv6RaPrefixEnable     TruthValue,
                    netifLanIpv6RaPrefixType       InetAddressType,
                    netifLanIpv6RaPrefixAddr       InetAddress,
                    netifLanIpv6RaPrefixLength     InetAddressPrefixLength,
                    netifLanIpv6RaPrefixOnlink     TruthValue,
                    netifLanIpv6RaPrefixAuto       TruthValue,
                    netifLanIpv6RaPrefixVLT        Unsigned32,
                    netifLanIpv6RaPrefixPLT        Unsigned32
                }

            netifLanIpv6RaPrefixEnable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/Disable the manually configured prefix(static 
                     prefix) for Router Advertisement on this interface.
                     The static prefix has priority over the one obtained 
                     via IA_PD."
                ::= { netifLanIpv6RaPrefixEntry 1 }
            
            netifLanIpv6RaPrefixType OBJECT-TYPE
                SYNTAX      InetAddressType
                MAX-ACCESS  read-only -- always ipv6
                STATUS      current
                DESCRIPTION
                    "Internet address type of the prefix. Must be ipv6 here
                    ."
                ::= { netifLanIpv6RaPrefixEntry 2 }

            netifLanIpv6RaPrefixAddr OBJECT-TYPE
                SYNTAX      InetAddress
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Internet address value of the prefix."
                ::= { netifLanIpv6RaPrefixEntry 3 }
            
            netifLanIpv6RaPrefixLength OBJECT-TYPE
                SYNTAX      InetAddressPrefixLength
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Prefix length."
                ::= { netifLanIpv6RaPrefixEntry 4 }

            netifLanIpv6RaPrefixOnlink OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "If true, indicates that this prefix can be used for 
                    on-link determination. If false, the advertisement
                    makes no statement about on-link or off-link properties
                    of the prefix. "
                ::= { netifLanIpv6RaPrefixEntry 5 }

            netifLanIpv6RaPrefixAuto OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "If true, indicates that this prefix can be used for 
                    autonomous address configuration as specified in RFC 
                    4862."
                ::= { netifLanIpv6RaPrefixEntry 6 }

            netifLanIpv6RaPrefixVLT OBJECT-TYPE
                SYNTAX      Unsigned32
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "The length of time in seconds that the prefix is valid
                    for the purpose of on-link determination. Check RFC4862
                    for more details."
                ::= { netifLanIpv6RaPrefixEntry 7 }

            netifLanIpv6RaPrefixPLT OBJECT-TYPE
                SYNTAX      Unsigned32
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "The length of time in seconds that addresses generated
                    from the prefix via stateless address autoconfiguration
                    remain preferred. Check RFC4862 for more details. "
                ::= { netifLanIpv6RaPrefixEntry 8 }

        -- IPv6 Router Advertisement manually configured DNS

            netifLanIpv6RaDnsTable OBJECT-TYPE
                SYNTAX      SEQUENCE OF NetifLanIpv6RaDnsEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "This table is used to configure the DNS advertised on 
                    LAN interface."
            ::= { netifLan 6 }
            
                netifLanIpv6RaDnsEntry OBJECT-TYPE
                    SYNTAX      NetifLanIpv6RaDnsEntry
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "Entry of the netifLanIpv6RaDnsTable."
                    INDEX       { netifLanBaseType,
                                  netifLanBaseIndex,
                                  netifLanIpv6RaDnsIndex }
                    ::= { netifLanIpv6RaDnsTable 1 }
    
                NetifLanIpv6RaDnsEntry ::=
                    SEQUENCE {
                        netifLanIpv6RaDnsIndex      Integer32,
                        netifLanIpv6RaDnsAddrType   InetAddressType,
                        netifLanIpv6RaDnsAddrValue  InetAddress
                    }
    
                netifLanIpv6RaDnsIndex OBJECT-TYPE
                    SYNTAX      Integer32
                    MAX-ACCESS  not-accessible
                    STATUS      current
                    DESCRIPTION
                        "The index of the manually configured DNS."
                    ::= { netifLanIpv6RaDnsEntry 1 }
                
                netifLanIpv6RaDnsAddrType OBJECT-TYPE
                    SYNTAX      InetAddressType
                    MAX-ACCESS  read-only -- always ipv6
                    STATUS      current
                    DESCRIPTION
                        "Internet address type of the manually configured 
                        DNS."
                    ::= { netifLanIpv6RaDnsEntry 2 }
    
                netifLanIpv6RaDnsAddrValue OBJECT-TYPE
                    SYNTAX      InetAddress
                    MAX-ACCESS  read-write
                    STATUS      current
                    DESCRIPTION
                        "Internet address value of the manually configured 
                        DNS."
                    ::= { netifLanIpv6RaDnsEntry 3 }

        -- LAN DHCPv6 Server Table(extension to the LAN interface Table)
        netifLanDhcpv6ServerTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF NetifLanDhcpv6ServerEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "LAN interface DHCPv6 Server table"
            ::= { netifLan 7 }

            netifLanDhcpv6ServerEntry OBJECT-TYPE
                SYNTAX      NetifLanDhcpv6ServerEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Entry of the DHCPv6 Server Table"
                AUGMENTS    { netifLanBaseEntry }
                ::= { netifLanDhcpv6ServerTable 1 }
    
            NetifLanDhcpv6ServerEntry ::= 
                SEQUENCE {
                    netifLanDhcpv6ServerEnable      TruthValue,
                    netifLanDhcpv6ServerRapidCommit TruthValue
                }
    
            netifLanDhcpv6ServerEnable OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "Enable/disable the DHCPv6 Server on the particular LAN
                    interface."
                ::= { netifLanDhcpv6ServerEntry 1 }
            
             netifLanDhcpv6ServerRapidCommit OBJECT-TYPE
                SYNTAX      TruthValue
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "If true, the DHCPv6 server will handle Rapid-Commit 
                    option ."
                ::= { netifLanDhcpv6ServerEntry 2 }
        
        -- IPv4 MAC-IP binding (LAN child table)
        netifLanMacIpBindTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF NetifLanMacIpBindEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "This table is used to configure the reserved IP address
                for particular MAC address in DHCPv4 pool."
            ::= { netifLan 8 }
        
            netifLanMacIpBindEntry OBJECT-TYPE
                SYNTAX      NetifLanMacIpBindEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Entry of the netifLanMacIpBindTable."
                INDEX       { netifLanBaseType,
                              netifLanBaseIndex,
                              netifLanMacIpBindIndex }
                ::= { netifLanMacIpBindTable 1 }
    
            NetifLanMacIpBindEntry ::=
                SEQUENCE {
                    netifLanMacIpBindIndex          Integer32,
                    netifLanMacIpBindRowStatus      RowStatus,
                    netifLanMacIpBindDescription    DisplayString,
                    netifLanMacIpBindHostName       DisplayString,
                    netifLanMacIpBindMacAddress     MacAddress,
                    netifLanMacIpBindIpAddress      IpAddress
                }
    
            netifLanMacIpBindIndex OBJECT-TYPE
                SYNTAX      Integer32
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Index of the MAC-IP binding table."
                ::= { netifLanMacIpBindEntry 1 }
    
            netifLanMacIpBindRowStatus OBJECT-TYPE
                SYNTAX      RowStatus
                MAX-ACCESS  read-create
                STATUS      current
                DESCRIPTION
                    "RowStatus used to manage the creation and deletion of 
                    conceptual row. "
                ::= { netifLanMacIpBindEntry 2 }
    
            netifLanMacIpBindDescription OBJECT-TYPE
                SYNTAX      DisplayString
                MAX-ACCESS  read-create
                STATUS      current
                DESCRIPTION
                    "Description of this MAC-IP binding entry."
                DEFVAL      { "New Mac-IP binding" }
                ::= { netifLanMacIpBindEntry 3 }
                
            netifLanMacIpBindHostName   OBJECT-TYPE
                SYNTAX      DisplayString
                MAX-ACCESS  read-create
                STATUS      current
                DESCRIPTION
                    "Name of the host with the MAC address."
                DEFVAL      { "Unknow Host" }
                ::= { netifLanMacIpBindEntry 4 }
    
            netifLanMacIpBindMacAddress OBJECT-TYPE
                SYNTAX      MacAddress
                MAX-ACCESS  read-create
                STATUS      current
                DESCRIPTION
                    "MAC address used to binding."
                ::= { netifLanMacIpBindEntry 5 }
    
            netifLanMacIpBindIpAddress OBJECT-TYPE
                SYNTAX      IpAddress
                MAX-ACCESS  read-create
                STATUS      current
                DESCRIPTION
                    "IPv4 address used to binding."
                ::= { netifLanMacIpBindEntry 6 }

        -- LAN Rip Table(extension to the LAN interface Table)
        netifLanRipTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF NetifLanRipEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "LAN interface Rip table"
            ::= { netifLan 9 }

            netifLanRipEntry OBJECT-TYPE
                SYNTAX      NetifLanRipEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Entry of the Rip Table"
                AUGMENTS    { netifLanBaseEntry }
                ::= { netifLanRipTable 1 }
    
            NetifLanRipEntry ::= 
                SEQUENCE {
                    netifLanRipSend          INTEGER,
                    netifLanRipReceive       INTEGER,
                    netifLanRipAuthType      INTEGER,
                    netifLanRipAuthKey       OCTET STRING,
                    netifLanRipAuthId        INTEGER
                }
            netifLanRipSend OBJECT-TYPE
                SYNTAX   INTEGER {
                            rip1 (1),
                            rip2 (2),
                            rip1orRip2 (3),
                            doNotSend (4)
                         }
                MAX-ACCESS  read-create
                STATUS      current
                DESCRIPTION
                   "What the router sends on this interface.
                   ripVersion1 implies sending RIP updates compliant
                   with  RFC  1058.   ripVersion2 implies
                   multicasting RIP-2 updates.  rip1orRip2 indicates
                   the use of  RIP on a This interface RIP-1 and RIP-2
                   updates."
               DEFVAL { doNotSend }
               ::= { netifLanRipEntry 1 }

            netifLanRipReceive OBJECT-TYPE
                SYNTAX   INTEGER {
                            rip1 (1),
                            rip2 (2),
                            rip1orRip2 (3),
                            doNotRecieve (4)
                         }
                MAX-ACCESS   read-create
                STATUS   current
                DESCRIPTION
                   "This indicates which version of RIP updates
                   are to be accepted."
               DEFVAL { doNotRecieve }
               ::= { netifLanRipEntry 2 }

            netifLanRipAuthType OBJECT-TYPE
                SYNTAX   INTEGER {
                            noAuthentication (1),
                            simplePassword (2),
                            md5 (3)
                         }
                MAX-ACCESS   read-create
                STATUS   current
                DESCRIPTION
                   "The type of Authentication used on this
                   interface."
               DEFVAL { noAuthentication }
               ::= { netifLanRipEntry 3 }

           netifLanRipAuthKey OBJECT-TYPE
                SYNTAX      OCTET STRING (SIZE(0..16))
                MAX-ACCESS   read-create
                STATUS   current
                DESCRIPTION
                   "The value to be used as the Authentication Key
                   whenever the corresponding instance of
                   netifLanRipAuthType has a value other than
                   noAuthentication.  A modification of the corresponding
                   instance of netifLanRipAuthType does not modify
                   the rip2IfConfAuthKey value.  If a string shorter
                   than 16 octets is supplied, it will be left-
                   justified and padded to 16 octets, on the right,
                   with nulls (0x00).

                   Reading this object always results in an  OCTET
                   STRING of length zero; authentication may not
                   be bypassed by reading the MIB object."
               DEFVAL { ''h }
               ::= { netifLanRipEntry 4 }

            netifLanRipAuthId OBJECT-TYPE
                SYNTAX      INTEGER
                MAX-ACCESS  read-create
                STATUS      current
                DESCRIPTION
                   "The value to be used as the Authentication Id
                   whenever the corresponding instance of
                   netifLanRipAuthType is md5."
               ::= { netifLanRipEntry 5 }

