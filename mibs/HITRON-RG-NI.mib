        -- Wan Rip Table(extension to the Wan interface Table)
        netifWanRipTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF NetifWanRipEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "Wan interface Rip table"
            ::= { netifWan 9 }

            netifWanRipEntry OBJECT-TYPE
                SYNTAX      NetifWanRipEntry
                MAX-ACCESS  not-accessible
                STATUS      current
                DESCRIPTION
                    "Entry of the Rip Table"
                AUGMENTS    { netifWanBaseEntry }
                ::= { netifWanRipTable 1 }
    
            NetifWanRipEntry ::= 
                SEQUENCE {
                    netifWanRipSend             INTEGER,
                    netifWanRipReceive          INTEGER,
                    netifWanRipDefaultMetric    INTEGER,
                    netifWanRipAuthType         INTEGER,
                    netifWanRipAuthKey          OCTET STRING,
                    netifWanRipAuthId           INTEGER,
                    netifWanRipUpdateInterval   Unsigned32
                }
            netifWanRipSend OBJECT-TYPE
                SYNTAX   INTEGER {
                            rip1 (1),
                            rip2 (2),
                            rip1orRip2 (3),
                            doNotSend (4)
                         }
                MAX-ACCESS  read-create
                STATUS      current
                DESCRIPTION
                   "What the router sends on this interface.
                   ripVersion1 implies sending RIP updates compliant
                   with  RFC 1058.   ripVersion2 implies
                   multicasting RIP-2 updates.  rip1orRip2 indicates
                   the use of  RIP on a This interface RIP-1 and RIP-2
                   updates."
               DEFVAL { doNotSend }
               ::= { netifWanRipEntry 1 }

            netifWanRipReceive OBJECT-TYPE
                SYNTAX   INTEGER {
                            rip1 (1),
                            rip2 (2),
                            rip1orRip2 (3),
                            doNotRecieve (4)
                         }
                MAX-ACCESS   read-create
                STATUS   current
                DESCRIPTION
                   "This indicates which version of RIP updates
                   are to be accepted."
               DEFVAL { doNotRecieve }
               ::= { netifWanRipEntry 2 }

            netifWanRipDefaultMetric OBJECT-TYPE
                SYNTAX   INTEGER ( 0..15 )
                MAX-ACCESS   read-create
                STATUS   current
                DESCRIPTION
                   "This variable indicates the metric that is to
                   be used for the default route entry in RIP updates
                   originated on this interface."
               ::= { netifWanRipEntry 3 }

            netifWanRipAuthType OBJECT-TYPE
                SYNTAX   INTEGER {
                            noAuthentication (1),
                            simplePassword (2),
                            md5 (3)
                         }
                MAX-ACCESS   read-create
                STATUS   current
                DESCRIPTION
                   "The type of Authentication used on this
                   interface."
               DEFVAL { noAuthentication }
               ::= { netifWanRipEntry 4 }

           netifWanRipAuthKey OBJECT-TYPE
                SYNTAX      OCTET STRING (SIZE(0..16))
                MAX-ACCESS   read-create
                STATUS   current
                DESCRIPTION
                   "The value to be used as the Authentication Key
                   whenever the corresponding instance of
                   netifWanRipAuthType has a value other than
                   noAuthentication.  A modification of the corresponding
                   instance of netifWanRipAuthType does not modify
                   the netifWanRipAuthKey value.  If a string shorter
                   than 16 octets is supplied, it will be left-
                   justified and padded to 16 octets, on the right,
                   with nulls (0x00).

                   Reading this object always results in an  OCTET
                   STRING of length zero; authentication may not
                   be bypassed by reading the MIB object."
               DEFVAL { ''h }
               ::= { netifWanRipEntry 5 }

            netifWanRipAuthId OBJECT-TYPE
                SYNTAX      INTEGER
                MAX-ACCESS  read-create
                STATUS      current
                DESCRIPTION
                   "The value to be used as the Authentication Id
                   whenever the corresponding instance of
                   netifWanRipAuthType is md5."
               ::= { netifWanRipEntry 6 }

            netifWanRipUpdateInterval OBJECT-TYPE
                SYNTAX      Unsigned32
                MAX-ACCESS  read-create
                STATUS      current
                DESCRIPTION
                   "The value to be used as Rip Update Interval."
