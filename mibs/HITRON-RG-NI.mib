                MAX-ACCESS  read-create
                STATUS      current
                DESCRIPTION
                   "Enable/Disable Ripng Receive on this Wan interface."
               ::= { netifWanRipngEntry 2 }


        netifWanOspfTable OBJECT-TYPE
            SYNTAX       SEQUENCE OF NetifWanOspfEntry
            MAX-ACCESS   not-accessible
            STATUS       current
            DESCRIPTION
               "Information about this router's virtual interfaces
               that the OSPF Process is configured to carry on."
            ::= { netifWan 11 }

            netifWanOspfEntry OBJECT-TYPE
                SYNTAX       NetifWanOspfEntry
                MAX-ACCESS   not-accessible
                STATUS       current
                DESCRIPTION
                  "Information about a single virtual interface.

                  Information in this table is persistent and when this object
                  is written the entity SHOULD save the change to non-volatile
                  storage."

               AUGMENTS    { netifWanBaseEntry }
               ::= { netifWanOspfTable 1 }

            NetifWanOspfEntry ::=
                SEQUENCE {
                    netifWanOspfAreaId          IpAddress,
                    netifWanOspfRtrDeadInterval Unsigned32,
                    netifWanOspfAuthKey         OCTET STRING,
                    netifWanOspfAuthType        INTEGER,
                    netifWanOspfAuthId          INTEGER,
                    netifWanOspfCost            INTEGER
                  }

            netifWanOspfAreaId OBJECT-TYPE
                SYNTAX       IpAddress
                MAX-ACCESS   read-only 
                STATUS       current
                DESCRIPTION
                   "The transit area that the virtual link
                   traverses.  By definition, this is not 0.0.0.0."
                ::= { netifWanOspfEntry 1 }

            netifWanOspfRtrDeadInterval OBJECT-TYPE

                SYNTAX       Unsigned32
                MAX-ACCESS   read-create
                STATUS       current
                DESCRIPTION
                    "The number of seconds that a router's Hello
                    packets have not been seen before its
                    neighbors declare the router down.  This should be
                    some multiple of the Hello interval.  This
                    value must be the same for the virtual neighbor."
                DEFVAL { 60 }
                ::= { netifWanOspfEntry 2 }


            netifWanOspfAuthKey OBJECT-TYPE
                SYNTAX       OCTET STRING (SIZE(0..256))
                MAX-ACCESS   read-create
                STATUS       current
                DESCRIPTION
                  "The cleartext password used as an OSPF
                  authentication key when simplePassword security
                  is enabled.  This object does not access any OSPF
                  cryptogaphic (e.g., MD5) authentication key under
                  any circumstance.

                  If the key length is shorter than 8 octets, the
                  agent will left adjust and zero fill to 8 octets.

                  Unauthenticated interfaces need no authentication
                  key, and simple password authentication cannot use
                  a key of more than 8 octets.

                  Note that the use of simplePassword authentication
                  is NOT recommended when there is concern regarding
                  attack upon the OSPF system.  SimplePassword
                  authentication is only sufficient to protect against
                  accidental misconfigurations because it re-uses
                  cleartext passwords.  [RFC1704]

                  When read, ospfIfAuthKey always returns an octet
                  string of length zero."
               DEFVAL { '0000000000000000'H } -- 0.0.0.0.0.0.0.0
               ::= { netifWanOspfEntry 3 }


            netifWanOspfAuthType OBJECT-TYPE
                SYNTAX  INTEGER {
                               none (0),
                               simplePassword (1),
                               md5 (2)
                            }   
               MAX-ACCESS   read-create
               STATUS       current
               DESCRIPTION
                  "The authentication type specified for a virtual interface."
               DEFVAL { none } 
               ::= { netifWanOspfEntry 4 }

            netifWanOspfAuthId OBJECT-TYPE
                SYNTAX       INTEGER
                MAX-ACCESS   read-create
                STATUS       current
                DESCRIPTION
                  "The authentication Id specified for a virtual interface."
                ::= { netifWanOspfEntry 5 }

            netifWanOspfCost OBJECT-TYPE
                SYNTAX       INTEGER
                MAX-ACCESS   read-create
                STATUS       current
                DESCRIPTION
                  "The Cost specified for a virtual interface."
                ::= { netifWanOspfEntry 6 }
                        
    netifCommit    OBJECT IDENTIFIER  ::= { hitronRgNi 4 }        
        netifCommitAll  OBJECT-TYPE
            SYNTAX      TruthValue
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Commit all RgNi settings."
            ::= { netifCommit 1 }
  
END
