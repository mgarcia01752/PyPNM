    switchPortTable OBJECT-TYPE
            SYNTAX     SEQUENCE OF SwitchPortEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                    "A list of configuration entries for individually
                     switched ports."
            ::= { smcSwitchControl 1 }
    
    switchPortEntry OBJECT-TYPE
            SYNTAX     SwitchPortEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                    "Configuration parameters for an individually
                     switched port."
            INDEX   { switchPortIndex }
            ::= { switchPortTable 1 }
    
    SwitchPortEntry ::=
            SEQUENCE {
                    switchPortIndex
                            INTEGER,
                    switchPortLinkAdmin
                            INTEGER,
                    switchPortLinkStatus
                            INTEGER,
                    switchPortAutoNegotiation
                            INTEGER,
                    switchPortDuplexAdmin
                            INTEGER,
                    switchPortDuplexStatus
                            INTEGER,
                    switchPortSpeedAdmin
                            INTEGER,
                    switchPortSpeedStatus
                            INTEGER,
                    switchPortTxRateControl
                            INTEGER,
                    switchPortTxRate
                            INTEGER,
                    switchPortRxRateControl
                            INTEGER,
                    switchPortRxRate
                            INTEGER
            }
    
    switchPortIndex OBJECT-TYPE
            SYNTAX     INTEGER
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "The current port number."
            ::= { switchPortEntry 1 }
    
    switchPortLinkAdmin OBJECT-TYPE
            SYNTAX     INTEGER { down(0), up(1) }
            MAX-ACCESS read-write
            STATUS     current
