-- All representations of MAC addresses in this MIB Module
-- use, as a textual convention (i.e. this convention does
-- not affect their encoding), the data type:

        MacAddress ::= OCTET STRING (SIZE (6))    -- a 6 octet
                                                  -- address in the
                                                  -- "canonical" order
-- groups in the cs1000 MIB

        csSystem            OBJECT IDENTIFIER  ::= {cs1000 1} 
        csSystemInfo        OBJECT IDENTIFIER  ::= {csSystem 1}
        csStatus            OBJECT IDENTIFIER  ::= {cs1000 2}
        csStatusSummary     OBJECT IDENTIFIER  ::= {csStatus 1}
       
--*****************************************
--
-- CS System Group
--   CS System Information Group
--
--*****************************************

csSystemOsAndVersion OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory

                DESCRIPTION "Operating sytem name and version."
                REFERENCE   " -- "
        ::= { csSystemInfo 1 }

csSystemAppVersion OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory

                DESCRIPTION "CS application version."
                REFERENCE   " -- "
        ::= { csSystemInfo 2 }

--*****************************************
--
-- CS System Group
--   CS Network Information Table Group
--
--*****************************************

csNetworkInfoTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF CsNetworkInfoTableEntry
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION "The table of information for network interface
                             cards installed on CS1000."
                REFERENCE   " -- "
        ::= { csSystem 2 }

csNetworkInfoTableEntry OBJECT-TYPE
                SYNTAX  CsNetworkInfoTableEntry
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION "An entry (conceptual row) in the CS1000
                             network information table."
                REFERENCE   " -- "
                INDEX  { csNetworkIndex }
        ::= { csNetworkInfoTable 1}
 
CsNetworkInfoTableEntry ::= SEQUENCE 
       {
           csNetworkIndex         INTEGER,
           csNetworkDescription   DisplayString,
           csNetworkMacAddress    MacAddress,
           csNetworkIpAddress     IpAddress,
           csNetworkSubnet        IpAddress
       }

csNetworkIndex OBJECT-TYPE
                SYNTAX  INTEGER(1..3)
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION "The auxiliary variable used for identifying
                             instances of the columnar objects.  Here the
                             index is the network interface number."
                REFERENCE   " -- "
        ::= { csNetworkInfoTableEntry 1 }

csNetworkDescription OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION "The description of the NIC."
                REFERENCE   " -- "
        ::= { csNetworkInfoTableEntry 2 }

csNetworkMacAddress OBJECT-TYPE
                SYNTAX  MacAddress
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION "The MAC address of the NIC."
                REFERENCE   " -- "
        ::= { csNetworkInfoTableEntry 3 }

csNetworkIpAddress OBJECT-TYPE
                SYNTAX  IpAddress
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION "The IP address of the NIC."
                REFERENCE   " -- "
        ::= { csNetworkInfoTableEntry 4 }

csNetworkSubnet OBJECT-TYPE
                SYNTAX  IpAddress
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION "The subnet mask of the NIC."
                REFERENCE   " -- "
        ::= { csNetworkInfoTableEntry 5 }

--*****************************************
--
-- CS Status Group
--   CS Status Summary Group
--
--*****************************************

csCarouselMgrUp OBJECT-TYPE
                SYNTAX  INTEGER
                         {
                          down    (1),
                          up      (2)
                         }
                ACCESS  read-only
                STATUS  mandatory

                DESCRIPTION "The status of Carousel manager."
                REFERENCE   " -- "
        ::= { csStatusSummary 1 }

csNumberOfCarousels OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory

                DESCRIPTION "The number of Carousel."
                REFERENCE   " -- "
        ::= { csStatusSummary 2 }

csAggregateBitRate OBJECT-TYPE
                SYNTAX  Counter
                ACCESS  read-only
                STATUS  mandatory  

                DESCRIPTION "The aggregate bit rate of the carousel."
                REFERENCE   " -- "
        ::= { csStatusSummary 3 }

