            bgpNlriSafi,
            bgpNlriPrfx,
            bgpNlriPrfxLen,
            bgpPathAttrExtCommIndex }
    ::= { bgpPathAttrExtCommTable 1 }

  BgpPathAttrExtCommEntry ::= SEQUENCE {
      bgpPathAttrExtCommIndex     Unsigned32,
