       sipEndPntConfigUserCLIDEnabled   TruthValue,
       sipEndPntConfigUserVoiceMailURI  SnmpAdminString,
       sipEndPntConfigUserCSAURI        SnmpAdminString,
       sipEndPntConfigUserSubscribeMsgWaitPkg  TruthValue,
       sipEndPntConfigUserCustomRingbackOnNet  TruthValue,
       sipEndPntConfigUserDfltWarmLine  OCTET STRING,
