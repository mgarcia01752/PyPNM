-- ------------------------------------------------------------------------------
--
-- semLoadSemTable
--

semLoadSemTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SemLoadSemEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "A table of LOAD SEM Command status information.
         This table contains status information on every command
         currently stored within the SEM.  The table is ordered in
         such a way as the dependencies of Load SEM Commands can be
         extracted and a tree of commands can be created.  Commands
         that are not dependent upon other commands have a level of
         0.  Commands that are at the top of a dependency tree have
         a level of 1.  The structure of the rest of the dependency tree
         can be determined by the order of the entries to the table. The
         entries are ordered as if doing a depth first decent of
         the tree with the depth of each command indicated by its level.
         Therefore, if a command has a level other than 0 or 1 then
         the first previous command that has a level 1 less than it
         is the command to which it is dependent.
         Example:
         This partial table

           Index  Level ... sequenceNumber ...
            1        0             1
            2        1             2
            3        2             3
            4        3             4
            5        2             5
            6        1             6
            7        2             7

         translates to these trees of sequence numbers
                2           6
               / \          |
              3   5         7
            /
           4

         with sequence number 1 existing on its own not part of a tree.
        "
::= { semSysStatusCommands 2 }

semLoadSemEntry OBJECT-TYPE
    SYNTAX SemLoadSemEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Load SEM Table Entry."
    INDEX { semLoadSemIndex }
::= { semLoadSemTable 1 }

SemLoadSemEntry ::= SEQUENCE
    {
     semLoadSemIndex                Integer32,
     semLoadSemLevel                Integer32,
     semLoadSemState                INTEGER,
     semLoadSemSubState             INTEGER,
     semLoadSemSequenceNumber       Integer32,
     semLoadSemSubcommandId         Integer32,
     semLoadSemSubcommandErrorCode  Integer32,
     semLoadSemSubcommandRetryCount Integer32,
     semLoadSemCommandBlock1        OCTET STRING
    }

semLoadSemIndex OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The index of the Load SEM"
::= { semLoadSemEntry 1 }

semLoadSemLevel OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The level of the command in the tree of load SEM
         commands."
::= { semLoadSemEntry 2 }

semLoadSemState OBJECT-TYPE
    SYNTAX INTEGER
        {
         waitingForStart (1),
