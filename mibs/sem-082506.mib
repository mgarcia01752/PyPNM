SemUpConvConfigEntry ::= SEQUENCE
    {
     semUpConvConfigOutputNumber       Integer32,
     semUpConvConfigApplyChange        ApplyDataToDeviceTYPE,
     semUpConvConfigTuningMode         INTEGER,
     semUpConvConfigEiaChanNumQamA     Integer32,
