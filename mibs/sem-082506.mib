--
-- semIsaSdvQamStatTable
--

semIsaSdvQamStatTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SemIsaSdvQamStatEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Table of ISA Switched Digital Video QAM status.  This table
         is indexed by QAM port number and contains 12 rows.

         This table only applies when the SEM is in semDeviceOperatingMode
         'isaSdv'.
        "
::= { semIsaSdvStatus 3 }

semIsaSdvQamStatEntry OBJECT-TYPE
    SYNTAX SemIsaSdvQamStatEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "ISA Switched Digital Video QAM Status Table Entry."
    INDEX { semIsaSdvQamStatQamPortNum }
::= { semIsaSdvQamStatTable 1 }

SemIsaSdvQamStatEntry ::= SEQUENCE
    {
     semIsaSdvQamStatQamPortNum          Integer32,
     semIsaSdvQamStatQamEnabled          EnableDisableTYPE,
     semIsaSdvQamStatOutTransStreamNum   Integer32,
     semIsaSdvQamStatNumSdvSessions      Integer32,
     semIsaSdvQamStatNumVodBcSessions    Integer32,
     semIsaSdvQamStatSdvGroupBandwidth   Unsigned32
    }

semIsaSdvQamStatQamPortNum OBJECT-TYPE
    SYNTAX Integer32 (1..12)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The ISA Switched Digital Video QAM Status table index.
        "
::= { semIsaSdvQamStatEntry 1 }

semIsaSdvQamStatQamEnabled OBJECT-TYPE
    SYNTAX EnableDisableTYPE
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Indicates whether the QAM Port is enabled.
        "
::= { semIsaSdvQamStatEntry 2 }

semIsaSdvQamStatOutTransStreamNum OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The Output Transport Stream Number of the QAM Port (1-16).
        "
::= { semIsaSdvQamStatEntry 3 }

semIsaSdvQamStatNumSdvSessions OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The Number of reserved SDV sessions on this QAM Port. This is the
         number of SDV sessions that have been reserved by the manager. Each
         SDV session requires that a manager reserve a QAM. This is the count
         of SDV sessions reserved (not the actual number of active SDV sessions).
        "
::= { semIsaSdvQamStatEntry 4 }

semIsaSdvQamStatNumVodBcSessions OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The Number of VOD/Broadcast sessions on this QAM Port. This
         is the number of VOD/Broadcast sessions active on a QAM Port. Since
         VOD and Broadcast sessions are not required to be reserved for an
         output, this is the count of active VOD and Broadcast sessions on
         a specific QAM.
        "
::= { semIsaSdvQamStatEntry 5 }

semIsaSdvQamStatSdvGroupBandwidth OBJECT-TYPE
    SYNTAX Unsigned32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The Group BW for SDV sessions (not used for VOD/Broadcast sessions).
         This is the total amount of BW allocated for all SDV sessions on a Port.
         The total SDV BW for a port is defined by the session manager. The manager
         reserves this BW for future SDV sessions. This is not the BW of current
         active SDV sessions, but the total BW reserved by the manager for SDV sessions.
        "
::= { semIsaSdvQamStatEntry 6 }


