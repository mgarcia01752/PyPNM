    clockpllError  (1),
    ok             (2)
   }

ApplyDataToDeviceTYPE ::= INTEGER
   {
    applyNotInProgress            (1),
    apply                         (2),
    applyNotInProgressValidData   (3),
    applyNotInProgressInvalidData (4)
   }

UpdateStatusTYPE ::= INTEGER
   {
    updateNotInProgress (1),
    update              (2)
   }

ResetStatisticsTYPE ::= INTEGER
   {
    resetNotInProgress (1),
    reset              (2)
   }

ClearAlarmTYPE ::= INTEGER
   {
    clearAlarmNotInProgress (1),
    clearAlarm              (2)
   }

FlushTYPE ::= INTEGER
   {
    flushNotInProgress (1),
    flush              (2)
   }

EncryptedTYPE ::= INTEGER
   {
    clear     (1),
    encrypted (2)
   }

UtilizationSamplingTYPE ::= INTEGER
   {
    samplingComplete       (1),
    samplingIncomplete     (2)
   }

UtilizationErrorTYPE ::= INTEGER
   {
    noError               (1),
    alarmThresholdReached (2),
    overflow              (3)
   }

SemHostEnetTYPE ::= INTEGER
   {
    hostEnet1 (1),
    hostEnet2 (2)
   }

EntitlementTYPE ::=  INTEGER
   {
    notEntitled (1),
    entitled    (2)
   }

RedundancySelectionTYPE ::=  INTEGER
   {
    notApplicable (0),
    primary       (1),
    secondary     (2)
   }

MulticastJoinTYPE ::=  INTEGER
   {
    notApplicable   (0),
    noJoinOnOpen    (1),
    joinOnOpen      (2)
   }

RateComparisonTYPE ::=  INTEGER
   {
    notApplicable       (0),
    dataRate            (1),
    transportStreamRate (2)
   }

SuspendTYPE ::=  INTEGER
   {
    notApplicable   (0),
    notSuspended    (1),
    suspended       (2)
