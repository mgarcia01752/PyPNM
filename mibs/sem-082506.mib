        }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The current state of the Command.  "
::= { semLoadSemEntry 3 }

semLoadSemSubState OBJECT-TYPE
    SYNTAX INTEGER
        {
         none                      (1),
         deletingDependants        (2),
         waitingForDependantDelete (3),
         waitingForAck             (4),
         waitingForResponse        (5),
         suspended                 (6),
         waitingForExtractAck      (7),
         waitingForExtractData     (8),
         waitingForValidInputPsi   (9),
         reserved10                (10),
         rebooting                 (11),
         deletingInactive          (12),
         deletingActive            (13),
         deletingSuspended         (14),
         deletedAll                (15),
         deletingOne               (16)
        }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The current substate of the command.
         Command specific"
::= { semLoadSemEntry 4 }

semLoadSemSequenceNumber OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The sequence number from the LOAD SEM Message"
::= { semLoadSemEntry 5 }

semLoadSemSubcommandId OBJECT-TYPE
    SYNTAX Integer32 (1..4)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The subcommand id from the load SEM Message"
::= { semLoadSemEntry 6 }

semLoadSemSubcommandErrorCode OBJECT-TYPE
    SYNTAX Integer32
