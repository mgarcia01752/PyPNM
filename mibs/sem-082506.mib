-- ------------------------------------------------------------------------------
--
-- semInsertQueTable
--

semInsertQueTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SemInsertQueEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "A table of Insertion Queue subqueue information. This table
         shows what messages the SEM is currently inserting for each
         output.  The message type, sequence number, pid,
         program number, start time, and stop time are shown. This table
         is indexed by output id, and subqueue id. For each
         index the message type and sequence number are provided."
::= { semSysStatusCommands 3 }

semInsertQueEntry OBJECT-TYPE
    SYNTAX SemInsertQueEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Insertion Queue Table Entry."
    INDEX { semInsertQueSubqueueOutputId,semInsertQueSubqueueIndex,semInsertQueSubqueueSlotNumber }
::= { semInsertQueTable 1 }

SemInsertQueEntry ::= SEQUENCE
    {
     semInsertQueSubqueueOutputId         Integer32,
     semInsertQueSubqueueIndex            Integer32,
     semInsertQueMessageType              Integer32,
     semInsertQueInsLoadSemSeqNum         Integer32,
     semInsertQuePid                      Integer32,
     semInsertQueProgramNumber            Integer32,
     semInsertQueStartTime                Integer32,
     semInsertQueStopTime                 Integer32,
     semInsertQueFirstTimeInserted        Integer32,
     semInsertQueSubqueueSlotNumber       Integer32
    }

semInsertQueSubqueueOutputId OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The output id index.  "
::= { semInsertQueEntry 1 }

semInsertQueSubqueueIndex OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The Index of the Subqueue"
::= { semInsertQueEntry 2 }

semInsertQueMessageType OBJECT-TYPE
    SYNTAX Integer32 (0..255)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The message type of the message being inserted"
::= { semInsertQueEntry 3 }

semInsertQueInsLoadSemSeqNum OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The LOAD SEM sequence number of the command
         responsible for the insert"
::= { semInsertQueEntry 4 }

semInsertQuePid OBJECT-TYPE
    SYNTAX Integer32 (0..8191)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The PID of the message being inserted"
::= { semInsertQueEntry 5 }

semInsertQueProgramNumber OBJECT-TYPE
    SYNTAX Integer32 (0..65535)
    MAX-ACCESS read-only
