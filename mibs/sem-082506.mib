-- ------------------------------------------------------------------------------
--
-- semOutputTransport
--
-- semOutTransConfig
--
-- semOutTransConfigGeneral
--

semPcrOffset OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "The PCR Offset.  Maximum value is 131071.

         Once written, the change to this parameter will take immediate
         effect.  However, in order for the change to persist through
         subsequent reboots or power cycles, the change must be saved via
         the semSaveConfig parameter.

         @Range(min=0, max=131071)
         @Config(config=yes, reboot=no)
         @Save(semSaveConfig, value=2)
         @File(config.ini, type='ini')
        "
::= { semOutTransConfigGeneral 1 }

semOutTransHighLowSpeedMode OBJECT-TYPE
    SYNTAX INTEGER
        {
         highSpeed (1),
         lowSpeed  (2)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "SEM Output Transport Stream High or Low Speed Mode.
         The SEM can support a High Speed mode (limited to a maximum of four
         Output Transport Streams and limited to OTS 1-4) and a Low Speed mode
         (all OTS may be used).  The SEM will configure
         internal routing of resources based on the mode.
            1) High Speed Mode - The user is limited to use of Output Transport
               Streams 1 to 4, and can have data rates up to 206 Mbps for unencrypted
               streams.  Maximum encrypted data rates will vary depending on the
               capabilities of the Access Control Processors installed in the SEM.
               In High Speed Mode the user is not limited to configuring stream
               data rates of greater than 53 Mbps.
            2) Low Speed Mode - The user may use all Output Transport Streams but
               is limited to a maximum of 53 Mbps.  The streams may be encrypted
               or unencrypted.

         Once written, a save must be performed via the semSaveConfig
         parameter and the SEM must be rebooted for the change to take effect.

         @Config(config=yes, reboot=yes)
         @Save(semSaveConfig, value=2)
         @File(config.ini, type='ini')
        "
::= { semOutTransConfigGeneral 2 }


-- ------------------------------------------------------------------------------
--
-- semOutTransStreamTable
--

semOutTransStreamTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SemOutTransStreamEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This is a table of configuration parameters for the sixteen
         Output Transport Streams.

         @Config(config=yes, reboot=yes)
         @Save(semSaveConfig, value=2)
         @File(config.ini, type='ini')
        "
::= { semOutTransConfig 2 }

semOutTransStreamEntry OBJECT-TYPE
    SYNTAX SemOutTransStreamEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Output Transport Stream Configuration Table Entry."
    INDEX { semOutTransStreamStreamNum }
::= { semOutTransStreamTable 1 }

SemOutTransStreamEntry ::= SEQUENCE
    {
     semOutTransStreamStreamNum            Integer32,
     semOutTransStreamEnable               EnableDisableTYPE,
     semOutTransStreamPhysOutType          INTEGER,
     semOutTransStreamPhysOutPort          Integer32,
     semOutTransStreamClockAssigned        Integer32,
     semOutTransStreamNumEncryptPrograms   Integer32
    }

semOutTransStreamStreamNum OBJECT-TYPE
    SYNTAX Integer32(1..16)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Output Transport Stream Number."
::= { semOutTransStreamEntry 1 }

semOutTransStreamEnable OBJECT-TYPE
    SYNTAX EnableDisableTYPE
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "This parameter indicates that the Output Transport Stream
         is configured and in use.

         Once written, a save must be performed via the semSaveConfig
         parameter and the SEM must be rebooted for the change to take effect.
        "
::= { semOutTransStreamEntry 2 }

semOutTransStreamPhysOutType OBJECT-TYPE
    SYNTAX INTEGER
        {
         gbe (1),
         asi (2),
         qam (4)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "The physical interface type to which this Output Transport Stream
         is to be routed.  The settings are limited to the following:
           - gbe - for any stream 1-16
                   Notes on GbE:
                   SEM1000/SEM v8/SEM v4:
                   1) There are three GbE outputs on the SEM.
                   SEM v12:
                   1) There are two GbE outputs on the SEM.
           - asi - only for streams 1-4, depending on product
                   Notes on ASI:
                   SEM v12:
                   1) There are no ASI outputs on the SEM.
           - qam - for streams 1-16, depending on product
                   Notes on QAM:
                   SEM1000/SEM v8:  streams 9-16
                   1) There are four upconverters on the SEM.
                   2) Each upconverter has two QAM channels associated with it.
                   3) They are grouped such that Output Transport Streams
                      9 & 10 are associated with Upconverter 1,
                      OTS 11 & 12 with UC 2, OTS 13 & 14 with UC 3,
                      and 15 & 16 with UC 4.  These are referred to
                      as QAM1A, QAM1B, QAM2A, QAM2B, etc.
                   4) When only one QAM channel of UC pair is to be enabled, it
                      must be the first QAM of the pair, e.g., 9, 11, 13, and 15,
                      or the SEM will flag the configuration as invalid and disable
                      the Output Transport Stream.
                   5) The first channel of a QAM pair must be enabled when the
                      second channel is enabled or the SEM will flag the configuration
                      as invalid and disable the Output Transport Stream.  This is
                      necessary because the configuration of the first QAM channel
                      of a pair is used as a basis for configuring the second QAM
                      channel of the pair.
                   SEM v4:  streams 9, 11, 13, and 15
                   1) Is IF only and has a QAM module without an upconverter.
                   2) Is four channel.
                   3) Utilizes the 'A' channels of a SEM1000/SEM v8.  Therefore,
                      only Output Transport Streams 9, 11, 13, and 15 may be set
                      to QAM.
                   SEM v12:  streams 1-12
                   1) There are six upconverters on the SEM.
                   2) Each upconverter has two QAM channels associated with it.
                   3) They are grouped such that Output Transport Streams
                      1 & 2 are associated with Upconverter 1,
                      OTS 3 & 4 with UC 2, OTS 5 & 6 with UC 3,
                      OTS 7 & 8 with UC 4, OTS 9 & 10 with UC 5,
                      and OTS 11 & 12 with UC 6.  These are referred
                      to as QAM1A, QAM1B, QAM2A, QAM2B, etc.
                   4) When only one QAM channel of UC pair is to be enabled, it
                      must be the first QAM of the pair, e.g., 1, 3, 5, 7, 9, 11
                      or the SEM will flag the configuration as invalid and disable
                      the Output Transport Stream.
                   5) The first channel of a QAM pair must be enabled when the
                      second channel is enabled or the SEM will flag the configuration
                      as invalid and disable the Output Transport Stream.  This is
                      necessary because the configuration of the first QAM channel
                      of a pair is used as a basis for configuring the second QAM
                      channel of the pair.

         Once written, a save must be performed via the semSaveConfig
         parameter and the SEM must be rebooted for the change to take effect.
        "
::= { semOutTransStreamEntry 3 }

semOutTransStreamPhysOutPort OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Number of the output, of type configured by PhysOutType,
         to which this Output Transport Stream is routed.
         The user configured value is ignored by the SEM.
         For ASI and QAM the value is set by the SEM.
         Not used or set for GBE.

         @Config(config=no, reboot=no)
        "
::= { semOutTransStreamEntry 4 }

semOutTransStreamClockAssigned OBJECT-TYPE
    SYNTAX Integer32(0..8)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Number of the clock to use for this output transport stream.
         The SEM has eight clocks.
         This is used as the index into semOutputClockConfigTable.
         The clock rate must be configured (non-zero) in semOutputClockConfigTable
         to be a valid selection.
         Zero indicates no clock is assigned.  Zero can only be selected for disabled
         Output Transport Streams.  A clock must be assigned for an enabled Output
         Transport Stream.
         For dynamic configuration of the clock selection:
           - If the Output Transport Stream is enabled, the clock rate of the clock
             selected must be within the limits based on the semOutTransHighLowSpeedMode
             the SEM is in.
         Clock Assigned is not applicable or used for QAM output.  QAM rates
         are configured in qamConfig.

         Once written, the change to this parameter will take immediate
         effect.  However, in order for the change to persist through
         subsequent reboots or power cycles, the change must be saved via
         the semSaveConfig parameter.

         @Config(config=yes, reboot=no)
        "
::= { semOutTransStreamEntry 5 }

semOutTransStreamNumEncryptPrograms OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Number of programs that can be encrypted in the output transport stream.
         This parameter is subject to 2 constraints for this number. First only
         integer multiples of semAcpServicesPerCU are valid. Second the combined
         encrypted service count for all transports cannot exceed the acp modules
         capacity. Encrypted services shall be allocated on a first come first serve
         basis.

         This field is only applicable when the SEM is externally controlled
         and has an ACP Module installed.

         Once written, a save must be performed via the semSaveConfig
         parameter and the SEM must be rebooted for the change to take effect.

         @Range(min=0)
        "
::= { semOutTransStreamEntry 6 }


-- ------------------------------------------------------------------------------
--
-- semOutputClockConfigTable
--

semOutputClockConfigTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SemOutputClockConfigEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This is a table of configuration parameters for the eight
         SEM clocks."
::= { semOutTransConfig 3 }

semOutputClockConfigEntry OBJECT-TYPE
    SYNTAX SemOutputClockConfigEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Output Clock Configuration Table Entry."
    INDEX { semOutputClockConfigNum }
::= { semOutputClockConfigTable 1 }

SemOutputClockConfigEntry ::= SEQUENCE
    {
     semOutputClockConfigNum            Integer32,
     semOutputClockConfigRate           Integer32
    }

semOutputClockConfigNum OBJECT-TYPE
    SYNTAX Integer32(1..8)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Output Clock Number."
::= { semOutputClockConfigEntry 1 }

semOutputClockConfigRate OBJECT-TYPE
    SYNTAX Integer32 (0 | 1000000..206000000)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Rate for this clock in bits per second (bps).
         Zero indicates clock not in use.  Clock cannot be set to zero
         if it is in use by an enabled Output Transport Stream.
         Minimum is 1,000,000 bps.
         Maximum is 206,000,000 bps.
         Because of clock granularity, SEM clocks cannot be set to the
         exact value the user has selected.  The actual clock rate set
         in the SEM can be found in the semOutputClockStatusTable.
         Changing clock rates:
           - If a clock is not in use by an enabled Output Transport Stream
             any rate in the entire range can be selected.
           - If a clock is in use by an enabled Output Transport Stream
             the rate is limited based on the semOutTransHighLowSpeedMode
             the SEM is in.

         Once written, the change to this parameter will take immediate
         effect.  However, in order for the change to persist through
         subsequent reboots or power cycles, the change must be saved via
         the semSaveConfig parameter.

         @Config(config=yes, reboot=no)
         @Save(semSaveConfig, value=2)
         @File(config.ini, type='ini')
        "
::= { semOutputClockConfigEntry 2 }


