-- semDataIpMulticastRcvTable
--

semDataIpMulticastRcvTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SemDataIpMulticastRcvEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table is the Multicast Receive configuration table for the
         Data Ip Interface.  It is indexed by multicast receive index.

         Once written, the change to this table will only take immediate
         effect after semHostIpMulticastApplyChange is set to 'apply'.
        "
::= { semDataIpConfig 9 }

semDataIpMulticastRcvEntry OBJECT-TYPE
    SYNTAX SemDataIpMulticastRcvEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Data Ip Interface Multicast Receive Configuration Table Entry."
    INDEX { semDataIpMulticastRcvIndex }
::= { semDataIpMulticastRcvTable 1 }

SemDataIpMulticastRcvEntry ::= SEQUENCE
    {
     semDataIpMulticastRcvIndex    Integer32,
     semDataIpMulticastRcvIpAddr   IpAddress,
     semDataIpMulticastRcvUdpPort  Integer32
    }

semDataIpMulticastRcvIndex OBJECT-TYPE
    SYNTAX Integer32 (1..16)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The Multicast Receive table index."
::= { semDataIpMulticastRcvEntry 1 }

semDataIpMulticastRcvIpAddr OBJECT-TYPE
    SYNTAX IpAddress
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "The Multicast receive IP address (224.0.0.0 to 224.0.0.255 is reserved).
         An IP address of 0.0.0.0 indicates table entry not in use.

         Once written, the change to this table will only take immediate
         effect after semHostIpMulticastApplyChange is set to 'apply'.
         However, in order for the change to persist through subsequent
         reboots or power cycles, the change must be saved via the
         semSaveConfig parameter.

         @Commit(param=semHostIpMulticastApplyChange, value=2)
         @Config(config=yes, reboot=no)
         @Save(semSaveConfig, value=2)
         @File(config.ini, type='ini')
        "
::= { semDataIpMulticastRcvEntry 2 }

semDataIpMulticastRcvUdpPort OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "The Multicast receive UDP Port (1024 to 65535). The SEM will only
         use the new or changed UDP port for multicast data reception when
         the UDP port is first opened. If the UDP Port is currently in use
         for singlecast or broadcast reception, this change will not take
         affect until after the UDP port is closed and then reopened (all
         service and stream routes must be removed to close a UDP port).

         Once written, the change to this table will only take immediate
         effect after semHostIpMulticastApplyChange is set to 'apply'.
         However, in order for the change to persist through subsequent
         reboots or power cycles, the change must be saved via the
         semSaveConfig parameter.

         @Range(min=0, max=65535)
         @Commit(param=semHostIpMulticastApplyChange, value=2)
         @Config(config=yes, reboot=no)
         @Save(semSaveConfig, value=2)
         @File(config.ini, type='ini')
        "
::= { semDataIpMulticastRcvEntry 3 }


