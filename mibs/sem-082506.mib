    openedInUse         (1),
    openedBackup        (2),
    openedTransToBackup (3),
    openedTransToInUse  (4)
   }



--
-- SEM MIB STRUCTURE
--
-- The following duplicates the structure and entries in the SEM MIB.
-- Object identifiers for groups are defined here.
--
   semSys                                                 OBJECT IDENTIFIER ::= {sem 1}
      semSysConfig                                        OBJECT IDENTIFIER ::= {semSys 1}
         semSysConfigGeneral                              OBJECT IDENTIFIER ::= {semSysConfig 1}
--          semSaveConfig                                                       {semSysConfigGeneral 1}
--          semProductName                                                      {semSysConfigGeneral 2}
--          semDeviceOperatingMode                                              {semSysConfigGeneral 3}
--          semCommandRetryPeriod                                               {semSysConfigGeneral 4}
--          semInsertModeEntry                                                  {semSysConfigGeneral 5}
--          semBootMethod                                                       {semSysConfigGeneral 6}
--          semPsiDetectionEnabled                                              {semSysConfigGeneral 7}
--          semPsiDetectionTimeout                                              {semSysConfigGeneral 8}
--          semAutoRebootEnable                                                 {semSysConfigGeneral 9}
         semSysConfigTime                                 OBJECT IDENTIFIER ::= {semSysConfig 2}
--          semTimeSource                                                       {semSysConfigTime 1}
--          semUtcOffset                                                        {semSysConfigTime 2}
--          semTimeSourceInputPort                                              {semSysConfigTime 3}
--          semTimeSourceSntpServerSpecified                                    {semSysConfigTime 4}
--          semTimeSourceSntpServerIP                                           {semSysConfigTime 5}
--          semUserSuppliedTime                                                 {semSysConfigTime 6}
         semSysConfigController                           OBJECT IDENTIFIER ::= {semSysConfig 3}
--          semControllerIP                                                     {semSysConfigController 1}
--          semControllerPort                                                   {semSysConfigController 2}
--          semLoadDeviceReceivePort                                            {semSysConfigController 3}
