irtSysConfTacConfig OBJECT-TYPE
        SYNTAX      INTEGER
            {
              primaryOdvarcEnet(1),
              secondary530(2),
              primaryOdvarc530(3),
              secondaryEnet(4),
              locallyControlled(5)
            }
--      MAX-ACCESS  read-write
--      STATUS      current
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION 
            "This defines the TCI Access Control (TAC) used by the
             IRT-1000.  Values and meanings are:
             
             'primaryOdvarcEnet':  This is a primary IRT.  Input to this
                  IRT is via ODVARC.  Output is via Ethernet.
             'primaryOdvarc530': :  This is a primary IRT.  Input to 
                  this IRT is via ODVARC.  Output is via RS-530.
             'secondaryEnet':  This is a secondary IRT.  Input to this
                  IRT is Ethernet.
             'secondary530': :  This is a secondary IRT.  Input to this
                  IRT is via RS-530.
             'locallyControlled':  The TAC configuration is not in use. 
				The IRT is being locally controlled by an ACC-4000D."

REFERENCE   " -- "
    ::= { irtSysConfTac 1 }

irtSysConfTACemmProviderID OBJECT-TYPE
        SYNTAX      INTEGER (0..65535)
--      MAX-ACCESS  read-write
--      STATUS      current
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION 
            "This contains the EMM Provider ID to be used to extract 
             messages from the TAC stream.  The default value of this 
             object is to be defined.  This means that when an IRT is 
             first powered on, it will internally set this object to 
             this default value.  Normally, this value will be set 
             during the configuration process"
        REFERENCE   " -- "
    ::= { irtSysConfTac 2 }

irtSysConfTACStreamUDPPort OBJECT-TYPE
        SYNTAX      INTEGER (0..65535)
--      MAX-ACCESS  read-write
--      STATUS      current
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION 
            "This contains UDP port number used in communication from a
             primary IRT to secondary IRTs and OMs. The default value is 5457."
        REFERENCE   " -- "
    ::= { irtSysConfTac 3 }

irtSysConfMulticastMACaddress OBJECT-TYPE
        SYNTAX      DisplayString
--      MAX-ACCESS  read-write
--      STATUS      current
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION 
            "This object contains the Multicast Ethernet Address used in communication from a
             primary IRT to secondary IRTs and OMs.  This is a six byte binary value with the MSB first."
        REFERENCE   " -- "
    ::= { irtSysConfTac 4 }

