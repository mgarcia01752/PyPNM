irtSysConfControllerIP OBJECT-TYPE
        SYNTAX      IpAddress
--      MAX-ACCESS  read-write
--      STATUS      current
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION 
            "This contains the IP address of the local controller.
             This is most often an ACC-4000D, but may be some other
             device.  The IRT needs to have an IP address of the local
             controller to begin operations.  The default value for 
             this object is an IP address of '0.0.0.0' (which is an
             invalid IP Address).  This means that when an IRT is first 
             powered on, it will internally set this object to this 
             default value.  If the configuration fails to set this
             object to a non-zero IP address, the IRT will not begin
             normal operation."
        REFERENCE   " -- "
    ::= { irtSysConf  2 }


irtSysConfSrvNumEMMprovIDcount OBJECT-TYPE
        SYNTAX      INTEGER (0..32)
--      MAX-ACCESS  read-write
--      STATUS      current
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION 
            "A count that determines the number of valid entries in the 
             table of Service Number To Entitlement Management Message
             (EMM) Provider Ids. The count may range from 0 to 32. "
        REFERENCE   " -- "
    ::= { irtSysConf  3 }

irtSysConfSrvNumEMMprovIDTable OBJECT-TYPE
		SYNTAX		SEQUENCE OF IrtSysConfSrvNumEMMprovIDEntry
		ACCESS		not-accessible
		STATUS		mandatory
		DESCRIPTION  
            "A table containing entries that have two fields. The first 
             field is the Service Number. The second field is the EMM 
             Provider Id. The table is used to authorize Decrypt ACPs 
             on a per service basis. "
    ::= { irtSysConf 4}

irtSysConfSrvNumEMMprovIDEntry		OBJECT-TYPE
		SYNTAX		IrtSysConfSrvNumEMMprovIDEntry
		ACCESS		not-accessible
		STATUS		mandatory
		DESCRIPTION  "A row in the Service Number to EMM Provider ID Table"
		INDEX {irtSysConfSrvNum }
::= { irtSysConfSrvNumEMMprovIDTable 1}

IrtSysConfSrvNumEMMprovIDEntry ::= SEQUENCE
	{
		irtSysConfSrvNum			INTEGER,
		irtSysConfEMMprovID			INTEGER
	}

