    {
     apexAsiMonitorPortBladeNum         Integer32,
     apexAsiMonitorPortOutputTsNum      Integer32,
     apexAsiMonitorPortEncryption       INTEGER
    }

apexAsiMonitorPortBladeNum OBJECT-TYPE
    SYNTAX Integer32(1..8)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Blade Number."
::= { apexAsiMonitorPortConfigEntry 1 }

apexAsiMonitorPortOutputTsNum OBJECT-TYPE
    SYNTAX Integer32(0..256)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "The number of the output transport stream to route to the
         ASI Monitor Port.  Zero indicates that no stream is to be
         routed to the ASI Monitor port.
        "
::= { apexAsiMonitorPortConfigEntry 2 }

apexAsiMonitorPortEncryption OBJECT-TYPE
    SYNTAX INTEGER
        {
         notApplicable  (0),
         preEncryption  (1),
         postEncryption (2)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Selects pre or post encryption for the ASI monitor port. When 
         set to pre-encryption, the output of the ASI monitor will contain
         the data prior to the APEX encrypting the services. When set to
         post-encryption, the output of the ASI monitor will contain the
         data after the APEX has encrypted the services. 
         
         This setting only applies to the scrambled control bits and actual
         packet data of the MPEG packets. All ECMs and PSI messages are always
         output via the ASI monitor port.
        "
::= { apexAsiMonitorPortConfigEntry 3 }


