    SYNTAX SEQUENCE OF ApexGbeConfigInterfaceRedundancyForceEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Table of Force Failover commands for the GBE Interface Redundancy feature.
        "
::= { apexGbeConfigInterfaceRedundancy 2 }

apexGbeConfigInterfaceRedundancyForceEntry OBJECT-TYPE
    SYNTAX ApexGbeConfigInterfaceRedundancyForceEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "GBE Config Interface Redundancy Table Entry."
    INDEX { apexGbeConfIfRedundForceIndex }
::= { apexGbeConfigInterfaceRedundancyForceTable 1 }

