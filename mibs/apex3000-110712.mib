    {
     apexGbeConfIfRedundForceIndex    Integer32,
     apexGbeConfIfRedundForceFailover INTEGER
    }

apexGbeConfIfRedundForceIndex OBJECT-TYPE
    SYNTAX Integer32(1..12)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The index of the GBE Config Interface Redundancy Force Table."
::= { apexGbeConfigInterfaceRedundancyForceEntry 1 }

