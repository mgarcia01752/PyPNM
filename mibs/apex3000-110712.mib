apexUdpMapBlade5MulticastEntry OBJECT-TYPE
    SYNTAX ApexUdpMapBlade5MulticastEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "UDP Map Multicast Table Entry."
    INDEX { apexUdpMapBlade5MulticastIndex }
::= { apexUdpMapBlade5MulticastTable 1 }

ApexUdpMapBlade5MulticastEntry ::= SEQUENCE
    {
     apexUdpMapBlade5MulticastIndex           Integer32,
     apexUdpMapBlade5MulticastEnable          EnableDisableTYPE,
     apexUdpMapBlade5MulticastVirtualIpIndex  Integer32,
     apexUdpMapBlade5MulticastUdp             Integer32,
     apexUdpMapBlade5MulticastMcastIp         IpAddress,
     apexUdpMapBlade5MulticastSourceIp        IpAddress
    }

apexUdpMapBlade5MulticastIndex OBJECT-TYPE
    SYNTAX Integer32(1..256)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Index of UDP Map Multicast Stream Table."
::= { apexUdpMapBlade5MulticastEntry 1 }

apexUdpMapBlade5MulticastEnable OBJECT-TYPE
    SYNTAX EnableDisableTYPE
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Indicates whether this UDP Map Multicast entry is enabled or disabled.
        "
::= { apexUdpMapBlade5MulticastEntry 2 }

apexUdpMapBlade5MulticastVirtualIpIndex OBJECT-TYPE
    SYNTAX Integer32(0..32)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Index of the Virtual IP Table that corresponds to this entry.

         Range:
            '0'              = Not Applicable
            Virtual IP Index = 1-32
        "
::= { apexUdpMapBlade5MulticastEntry 3 }

apexUdpMapBlade5MulticastUdp OBJECT-TYPE
    SYNTAX Integer32(0..65535)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "GBE Input UDP Port.

         Range: 0-65535
        "
::= { apexUdpMapBlade5MulticastEntry 4 }

apexUdpMapBlade5MulticastMcastIp OBJECT-TYPE
    SYNTAX IpAddress
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "The Multicast receive IP address.
         An IP address of 0.0.0.0 indicates singlecast only.

         Multicast IP addresses 224.0.0.0 to 224.0.0.255 are reserved.
        "
::= { apexUdpMapBlade5MulticastEntry 5 }

apexUdpMapBlade5MulticastSourceIp OBJECT-TYPE
    SYNTAX IpAddress
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "This is the IP address of the source device. This field is only
         used when a multicast IP address is also specified.
        "
::= { apexUdpMapBlade5MulticastEntry 6 }


