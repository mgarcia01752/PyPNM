apexQamEntStatusHwSupport OBJECT-TYPE
    SYNTAX INTEGER
        {
         unknown      (0),
         notSupported (1),
         supported    (2),
         overridden   (3)
        }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Indicates whether HW supports assignment of Chassis QAM
         Channel Entitlements to this RF port.
         
         If 'notSupported', the HW must be converted before
         QAM channel entitlements can be applied.
         
         If 'overridden', it indicates entitlement checking is 
         bypassed.
        "
::= { apexQamEntStatusEntry 3 }

apexQamEntStatusMinChannels OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Minimum total QAM channels for this RF port.  This is a 'base' level
         of QAM channels and is HW dependant.
         The RF port cannot be authorized for less than this number.
         Chassis QAM Channel Entitlements are not included in this number.
         Chassis QAM Channel Entitlements are applied on top of this 'base'.
        "
::= { apexQamEntStatusEntry 4 }

apexQamEntStatusMaxChannels OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Maximum total QAM channels for this RF port.  This is a 'ceiling' level
         of QAM channels and is HW dependant.
         The RF port cannot be authorized for more than this number.
         Chassis QAM Channel Entitlements are applied up to this 'ceiling'.
        "
::= { apexQamEntStatusEntry 5 }

apexQamEntStatusTotalChannels OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Total QAM Channels authorized for use on this RF port.
         This is the sum of the 'base' (apexQamEntStatusMinChannels) and
         the assigned Chassis QAM Entitlements (apexQamEntConfigAssignChannels).
        "
::= { apexQamEntStatusEntry 6 }

