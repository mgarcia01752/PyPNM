    SYNTAX ApexQamBladeConfigShutdownEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Blade Shutdown Configuration Table Entry."
    INDEX { apexQamBladeConfigBladeNum }
::= { apexQamBladeConfigShutdownTable 1 }

ApexQamBladeConfigShutdownEntry ::= SEQUENCE
    {
     apexQamBladeConfigBladeNum      Integer32,
     apexQamBladeConfigShutdown      INTEGER
    }

