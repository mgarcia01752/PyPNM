apexImportFileRequest OBJECT-TYPE
    SYNTAX INTEGER
        {
         notApplicable            (0),
         importFileNotInProgress  (1),
         importFileRequest        (2),
         importFileInProgress     (3),
         importFileError          (4),
         importFileCompleted      (5)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "This object handles file import interactions between EM and APEX3000.
         This request is used to start a configuration file import. The APEX3000
         will request files from the EM to import. All configuration files known
         to the APEX3000 will be imported from the user specified directory.
         
         The configuration files to import are defined by the APEX3000. The names
         must match the internally stored configuration file names. The user must
         ensure that all configuration files to import are correctly named and 
         stored in the user specified directory.
         
         'notApplicable'
          - default value (no import in progress)
          
         'importFileNotInProgress' 
          - no import in progress
          
         'importFileRequest' 
         - set by the EM to start a file import.
         
         'importFileInProgress' 
         - set by the APEX3000 when the File Import is in progress.

         'importFileError' 
         - set by the APEX3000 if some error makes the file import not possible. 
           Additional information is provided in apexImportFileStatusText.

         'importFileCompleted' 
         - set by the APEX3000 when the file import is completed.

         @Config(config=no, reboot=no)
        "
::= { apexImportFileConfigGeneral 1 }

apexImportFileOperation OBJECT-TYPE
    SYNTAX INTEGER
        {
         fileTransferRequestNotInProgress (0),
         fileTransferRequest              (1),
         fileIsReady                      (2),
         errorWhenUploadingfile           (3)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "This object is used by the APEX to request a file transfer during the upgrade procedure.

         'fileTransferRequestNotInProgress' 
         - default value.
 
         'fileTransferRequest' 
         - set by the APEX to request a file transfer. The filename should
           be previously set in the apexImportFileRequestedFile object.

         'fileIsReady' 
         - set by the EM once the file is uploaded correctly on the APEX.

         'errorWhenUploadingfile' 
         - set by the EM when some error occurs during the upload operation.

         @Config(config=no, reboot=no)
        "
::= { apexImportFileConfigGeneral 2 }

apexImportFileRequestedFile OBJECT-TYPE
    SYNTAX DisplayString (SIZE (0..127))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "This object is used by the APEX to indicate the requested file during 
         the file import procedure. It's formatted using the following syntax:

         'local_file_name, destination_path'.

         Where local_file_name refers to the local path (user side), and 
         destination path is the remote directory where the EM should upload the file.
        
         for example:

         'config.ini, /config1/config.ini'.

         Indicates to the EM that it should upload the file 'config.ini' to the destination
         '/config1/config.ini'.

         @Config(config=no, reboot=no)
        "
::= { apexImportFileConfigGeneral 3 }


