-- 
-- DECT extension MIBs
-- 

eueMgmtDect             OBJECT IDENTIFIER ::= { eueMgmtObject 1 }
eueMgmtDectFP           OBJECT IDENTIFIER ::= { eueMgmtDect 1 }

eueMgmtDectReset OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object controls the Dect reset.
         Reading this object always returns 'false'.  Setting this
         object to 'true' causes the Dect module to reset immediately
         and the following actions to occur:
             1.  All dect connections (if present) are flushed locally.
             2.  All current handset actions such as ringing immediately
                 terminate."
    ::= { eueMgmtDectFP 1 }

eueMgmtDectEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used to enable or disable the power of DECT.
         Setting this object to 'true' causes the Dect module to be powered 
         on if it was previously disabled (ie. powered off).
         Setting this object to 'false' causes the power of DECT to be disabled
         and the following actions to occur:
             1.  All dect connections (if present) are flushed locally.
             2.  All current handset actions such as ringing immediately
                 terminate.
             3.  All dect handsets are disabled.
         Note adding this MIB in the EDVA configuration file will override any
         build specific behavior related to the automatic shutdown/enabling of DECT."
    DEFVAL {true}
    ::= { eueMgmtDectFP 2 }

eueMgmtDectRegMode OBJECT-TYPE
    SYNTAX      INTEGER {
                unknown(0),
                register (1),
                deregister (2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used to configure the base to the registration mode.  
         Setting this object to 'register' enables Dect registration.
         Setting this object to 'deregister' causes all Dect handsets to be 
         deregistered.
         Note that reading this object always returns unknown(0), as this object 
         is used to send a command to the DECT and is not meant to reflect the 
         current registration mode of Dect.
         Note also that when the maximum number of Dect handsets have been
         registered, Dect registration cannot be enabled."
    ::= { eueMgmtDectFP 3 }

eueMgmtDectNumRegPP OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "PPs"
    MAX-ACCESS  read-only
    STATUS current
    DESCRIPTION
        "This attribute represents the number of PPs that are registered."
    ::= { eueMgmtDectFP 4 }

eueMgmtDectNumActivePP OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "PPs"
    MAX-ACCESS  read-only
    STATUS current
    DESCRIPTION
        "This attribute represents the number of PPs that are active."
    ::= { eueMgmtDectFP 5 }

eueMgmtDectMode  OBJECT-TYPE
    SYNTAX  INTEGER {
                  dectModeEuro(0),
                  dectModeUS(1),
                  dectModeSA(2),
                  dectModeTw(3),
                  dectModeMl(4),
                  dectModeCn(5),
                  dectModeTh(6),
                  dectModeBr(7),
                  dectModeExtUS(8),
                  dectModeUnknown(255)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The DECT mode (RF frequency) setting of the eUE:
            dectModeEuro(0) - Europe DECT mode
            dectModeUS(1) - United States DECT mode
            dectModeSA(2) - South America DECT mode
            dectModeTw(3) - Taiwan DECT mode
            dectModeMl(4) - Malaysia DECT mode
            dectModeCn(5) - China DECT mode
            dectModeTh(6) - Thailand DECT mode
            dectModeBr(7) - Brazil DECT mode
            dectModeExtUS(8) - United States DECT mode with extended freq band
            dectModeUnknown(255) - Unknown DECT mode        
          
         Note that this setting will take effect on the next eUE reset.
         The value dectModeUnknown is reported when the eUE does not yet know the operating
         mode of the DECT module. Any attempt to set this MIB to dectModeUnknown will result
         in an inconsistent value error."
     ::= { eueMgmtDectFP 6 }

eueMgmtDectPPTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF EueMgmtDectPPEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "This object represents the list of PPs and related
            identifiers.
            For debugging purposes this object lists more PPs than
            the maximum number of PPs."
    ::= {eueMgmtDect 2 }

eueMgmtDectPPEntry OBJECT-TYPE
    SYNTAX      EueMgmtDectPPEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The Conceptual row of eueMgmtDectPPTable."
    INDEX { pktcDectPPId }
    ::= {eueMgmtDectPPTable 1 }

EueMgmtDectPPEntry ::= SEQUENCE {
    eueMgmtDectPPStatus  INTEGER
    }
    
pktcDectPPId OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "Was undefined index"
    ::= { eueMgmtDectPPEntry 9999 }

eueMgmtDectPPStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                inRange(1),
                outOfRange(2),
                onHook(3),
                offHook(4),
                dataServiceInUse(5)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "This attribute represents the state of a PP."
    ::= {eueMgmtDectPPEntry 1 }


--
-- eueMgmtEDVAEndpoint - objects for EDVA endpoint config
--

eueMgmtEDVAEndpointConfigTable    OBJECT-TYPE
    SYNTAX        SEQUENCE OF EueMgmtEDVAEndpointConfigEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "This table describes configuration data pertaining   
 	      for each end point (telephnony line). 
 	      The number of entries in this table represents the   
 	      number of provisioned end points.
         This table augments the pktcEDVAEndpointConfigTable."
    ::=  { eueMgmtEDVAEndpoint 1 }

eueMgmtEDVAEndpointConfigEntry    OBJECT-TYPE
    SYNTAX       EueMgmtEDVAEndpointConfigEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "This Object represents the conceptual row in  
 	      the eueMgmtEDVAEndpointConfigTable.  
 	
 	      Each entry describes the configuration data for 
 	      the associated line. "
    AUGMENTS{ pktcEDVAEndPntConfigEntry }
    ::=  { eueMgmtEDVAEndpointConfigTable 1 }

EueMgmtEDVAEndpointConfigEntry  ::= SEQUENCE {
       eueMgmtEDVAEndpointConfigFaxOnly         TruthValue,
       eueMgmtEDVAEndpointConfigFaxWaitTime     Unsigned32,
       eueMgmtEDVAEndpointConfigSDPParam        SnmpAdminString
    }

eueMgmtEDVAEndpointConfigFaxOnly  OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Controls whether this endpoint is a Fax-only line.
         When true, the line is Fax-only which means it is a line of lower rate 
         and does not support regular voice application.  When a fax-only line goes 
         off-hook and no modem or fax tones are detected within a specified period
         of time, the line is dropped to prevent the line from being used for regular voice."
    DEFVAL {false}
    ::= { eueMgmtEDVAEndpointConfigEntry 1 }

eueMgmtEDVAEndpointConfigFaxWaitTime  OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current    
    DESCRIPTION
        " The period of time for the EDVA to detect modem and fax tones when the line is 
          configured for fax-only.  If no modem or fax tones are detected within this period
          of time, the line is dropped to prevent the fax-only line from being used for
          regular voice"
    DEFVAL {20}
    ::= { eueMgmtEDVAEndpointConfigEntry 2 }

eueMgmtEDVAEndpointConfigSDPParam  OBJECT-TYPE
    SYNTAX  SnmpAdminString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the current negotiated SDP parameters of an active call.  This
         includes the codec, tel-event, silence suppression, and rtcp-xr mode in the following
         format:  'Codec: [codec family] Opts [codec options], Tel-Event: [yes/no], 
                   Silence Suppression: [yes/no], RTCP-XR Negotiated: [yes/no]'
         If a call is not in progress on the endpoint, then 'call not in progress' will be 
         returned."
    ::= { eueMgmtEDVAEndpointConfigEntry 3 }


